<!-- Arrays could be defined with or without parenthesis and value can be separated both by , and by ; -->
<wing_executable>
        <operating_conditions>
                <altitude unit="m">0</altitude>
                <mach_number>0.13</mach_number>
        </operating_conditions>
       <wing>
            <global>
                <surface unit="m²">6.82837344</surface>
                <aspect_ratio>6</aspect_ratio>
                <number_of_point_semispan>100</number_of_point_semispan>
                <adimensional_kink_station>1</adimensional_kink_station>
             </global>
             <distribution>
                <number_of_given_sections>2</number_of_given_sections>
                <airfoil_family>NACA_64_SERIES</airfoil_family> 
                   <geometry>
						<y_adimensional_stations>[0,1]</y_adimensional_stations>
						<chord_distribution unit="m">[1.4224, 0.7112]</chord_distribution>
						<x_le_distribution unit="m">[0,2.433398847]</x_le_distribution>
						<twist_distribution unit="°">[0.0, 0.0]</twist_distribution>
						<max_thickness_distribution>[0.12,0.12]</max_thickness_distribution>
						<leading_edge_radius_distribution unit = "m">[0.15,0.15]</leading_edge_radius_distribution>
                    </geometry>
                    <aerodynamics>
						<alpha_zero_lift_distribution unit="°">[-1, -1]</alpha_zero_lift_distribution>
						<alpha_star_distribution unit="°">[8,8]</alpha_star_distribution>
						<cl0_distribution>[0.14, 0.14]</cl0_distribution>   
						<linear_slope_coefficient unit="1/°">[0.126, 0.126]</linear_slope_coefficient>
						<maximum_lift_coefficient_distribution>[1.15,1.13]</maximum_lift_coefficient_distribution>
						<alpha_stall_distribution unit="°">[12,11]</alpha_stall_distribution>
                    </aerodynamics>
             </distribution>
        </wing>
		<high_lift_devices>
		        <flaps>
					<flap id = "1">
						<flap_type unit="">DOUBLE_SLOTTED</flap_type>
						<flap_chord_ratio unit="">0.25</flap_chord_ratio>
