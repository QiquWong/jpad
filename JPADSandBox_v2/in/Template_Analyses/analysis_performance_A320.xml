<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <analysis>
        <performance
			id="A320 - PERFORMANCE"
			weights_from_previous_file="false" 
			aerodynamics_from_previous_file="false" 
			>
			<weights>
				<maximum_take_off_mass unit="kg">78000.0</maximum_take_off_mass>
				<operating_empty_mass unit="kg">42100.0</operating_empty_mass>
				<maximum_fuel_mass unit="kg">28861.0</maximum_fuel_mass>
				<maximum_payload unit="kg"></maximum_payload> <!-- default = design_n_pax*121kg + 700kg -->
				<single_passenger_mass unit="kg">92.0</single_passenger_mass>
			</weights>
			<aerodynamics>
				<xcg value="0.25" parabolicDragPolar="TRUE"> <!-- if TRUE uses a parabolic drag polar with wave drag correction; otherwise it reads the polar_curves tag -->
					<cLmax_clean_configuration>1.5</cLmax_clean_configuration>
					<cL_alpha_clean_configuration unit="1/deg">0.09</cL_alpha_clean_configuration>
					<cLmax_take_off_configuration>2.2</cLmax_take_off_configuration>
					<cL_alpha_take_off unit="1/deg">0.0905</cL_alpha_take_off>
					<cL0_take_off_configuration>0.8</cL0_take_off_configuration>
					<cLmax_landing_configuration>2.8</cLmax_landing_configuration>
					<cL_alpha_landing unit="1/deg">0.0925</cL_alpha_landing>
					<cL0_landing_configuration>1.25</cL0_landing_configuration>
					<delta_CD0_flap_take_off>0.015</delta_CD0_flap_take_off>
					<delta_CD0_flap_landing>0.085</delta_CD0_flap_landing>
					<delta_CD0_landing_gears>0.015</delta_CD0_landing_gears>
					<!-- the following data are used only if parabolicDragPolar flag is set to TRUE. In any other case, they can be left blank -->
					<cD0>0.0214</cD0>
					<oswald_cruise>0.82</oswald_cruise> 
					<oswald_climb>0.8</oswald_climb>
					<oswald_take_off>0.79</oswald_take_off>
					<oswald_landing>0.78</oswald_landing>
					<!---->
					<polar_curves>
						<cruise>
							<polar_curve_CL></polar_curve_CL>
							<polar_curve_CD></polar_curve_CD>
						</cruise>
						<climb>
							<polar_curve_CL></polar_curve_CL>
							<polar_curve_CD></polar_curve_CD>
						</climb>
						<take_off>
							<polar_curve_CL></polar_curve_CL>
							<polar_curve_CD></polar_curve_CD>
						</take_off>
						<landing>
							<polar_curve_CL></polar_curve_CL>
							<polar_curve_CD></polar_curve_CD>
						</landing>
					</polar_curves>
					<tau_rudder_function calculate="TRUE">
						<tau>0.025</tau>
						<delta_rudder unit="deg"></delta_rudder> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_rudder_function>					
				</xcg>
			</aerodynamics>
			<!-- the following data can be assigned between tags, or not. If not, default values are used -->
			<takeoff_landing>
				<!-- general data -->
				<wind_speed_along_runway unit="m/s"></wind_speed_along_runway> <!-- positive if behind the aircraft (default = 0m/s)-->
					<!-- here is possible to assign the fricition coefficient function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_function>
					<friction_coefficient>0.025</friction_coefficient>
					<speed unit="m/s"></speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_function>
					<!-- here is possible to assign the fricition coefficient (with brakes) function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_with_brakes_function>
					<friction_coefficient_with_brakes>0.3</friction_coefficient_with_brakes>
					<speed unit="m/s"></speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_with_brakes_function>
				<!-- take-off data -->
				<dt_hold unit="s"></dt_hold> <!-- the duration of the constant CL phase (default = 0.5s) -->
				<alpha_ground unit="deg"></alpha_ground> <!-- angle of attack in BRF along the runway (default = 0Â°)-->
				<obstacle_take_off unit="ft"></obstacle_take_off> <!-- default = 35 ft -->
				<k_rotation></k_rotation> <!-- V_rotation/V_stall in take-off (default = 1.05) -->
				<alpha_dot_rotation></alpha_dot_rotation> <!-- rotation angular velocity (must be in deg/s) (default = 3.0) -->
				<k_cLmax_take_off></k_cLmax_take_off> <!-- percentage of the CLmax,TO not to be surpassed during the rotation  (default = 0.9)-->
				<drag_due_to_engine_failure>0.005</drag_due_to_engine_failure> <!-- drag coefficient to be added due to the failed engine and rudder deflection (default = 0.0) -->
				<k_alpha_dot></k_alpha_dot> <!-- indicates how fast the rotation maneuver is - faster with decreasing k_alpha_dot - (default 0.04) -->
				<!-- landing data -->
				<k_landing_weight>0.9</k_landing_weight> <!-- percentage of the maximum take-off weight to be used for the landing performance - (default = 0.97) -->
				<initial_altitude_landing unit="ft"></initial_altitude_landing> <!-- default = 1500ft -->
				<obstacle_landing unit="ft"></obstacle_landing> <!-- default = 50ft -->
				<k_cLmax_landing></k_cLmax_landing> <!-- percentage of the CLmax,LND not to be surpassed during the flare rotation  (default = 0.9)-->
				<k_approach>1.23</k_approach> <!-- V_approach/V_stall in landing (default = 1.3) -->
				<k_flare></k_flare> <!-- V_flare/V_stall in landing (default = 1.2) -->
				<k_touch_down></k_touch_down> <!-- V_touch_down/V_stall in landing (default = 1.15) -->
				<free_roll_duration unit="s"></free_roll_duration> <!-- default = 2s -->
			</takeoff_landing>
			<climb>
				<k_climb_weight_AEO></k_climb_weight_AEO> <!-- percentage of the maximum take-off weight to be used for the climb performance (AOE) - (default = 1.0) -->
				<k_climb_weight_OEI></k_climb_weight_OEI> <!-- percentage of the maximum take-off weight to be used for the climb performance (OEI) - (default = 0.97) -->
				<initial_climb_altitude unit="ft">0.0</initial_climb_altitude>
				<final_climb_altitude unit="ft">39000.0</final_climb_altitude>
				<climb_speed_CAS unit="kn">190</climb_speed_CAS>
			</climb>
			<cruise>
				<altitudes_array unit="ft">10000;20000;30000;40000</altitudes_array>
				<k_cruise_weight>0.95</k_cruise_weight> <!-- percentage of the maximum take-off weight to be used for the cruise performance - (default = 0.98) -->
			</cruise>
			<descent>
				<rate_of_descent unit="ft/min">1500</rate_of_descent>
				<descent_speed_CAS unit="kn">220</descent_speed_CAS>
				<k_descent_weight></k_descent_weight> <!-- percentage of the maximum take-off weight to be used for the descent performance - (default = 0.9) -->
				<initial_descent_altitude unit="ft">39000</initial_descent_altitude>
				<final_descent_altitude unit="ft">50</final_descent_altitude>
			</descent>
			<flight_maneuvering_and_gust_envelope>
				<cLmax_inverted></cLmax_inverted> <!-- to be assigned for the manoeuvring flight and gust envelope (default = -1.0) --> 
			</flight_maneuvering_and_gust_envelope>
			<noise_trajectories>
				<take_off>
					<ground_distance_end_simulation unit="m"></ground_distance_end_simulation> <!-- (default = 8000.0 m) -->
					<cutback_altitude unit="ft"></cutback_altitude> <!-- (default = 300m/984ft) -->
					<dt_landing_gear_retraction unit="s"></dt_landing_gear_retraction> <!-- (default = 12s) -->
					<dt_thrust_cutback unit="s"></dt_thrust_cutback> <!-- (default = 4s) -->
				</take_off>
				<landing>
					<initial_altitude unit="ft"></initial_altitude> <!-- (default = 4000ft -->
				</landing>
			</noise_trajectories>
			<mission_profile_and_payload_range>
				<!-- if alternate and holding phases have not to be taken into account, leave their tags empty -->
				<mission_range unit="nmi">2500</mission_range> 
				<alternate_cruise_range unit="nmi">100</alternate_cruise_range> <!-- (default = 0.0) -->
				<alternate_cruise_altitude unit="ft">15000</alternate_cruise_altitude> <!-- (default = 15.24 = landing obstacle) -->
				<holding_duration unit="min">30</holding_duration> <!-- (default = 0.0) -->
				<holding_altitude unit="ft">1500</holding_altitude> <!-- (default = 0.0) -->
				<fuel_reserve>0.05</fuel_reserve> <!-- (default = 0.0) -->
			</mission_profile_and_payload_range>
        </performance>
		<calibrations> <!-- default values = 1.0 -->
			<thrust>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<continuous_calibration_factor></continuous_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</thrust>
			<sfc>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</sfc>
			<emission_index_NOx>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_NOx>
			<emission_index_CO>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_CO>
			<emission_index_HC>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_HC>
			<emission_index_Soot>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_Soot>
			<emission_index_CO2>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_CO2>
			<emission_index_SOx>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_SOx>
			<emission_index_H2O>
				<take_off_calibration_factor></take_off_calibration_factor>
				<apr_calibration_factor></apr_calibration_factor>
				<climb_calibration_factor></climb_calibration_factor>
				<cruise_calibration_factor></cruise_calibration_factor>
				<flight_idle_calibration_factor></flight_idle_calibration_factor>
				<ground_idle_calibration_factor></ground_idle_calibration_factor>
			</emission_index_H2O>
		</calibrations>
    </analysis>
	<plot>
		<takeoff>
			<simulations perform="TRUE"/>
			<balanced_field_length perform="TRUE"/>
			<vMC perform="TRUE"/>
		</takeoff>
		<climb>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<rate_of_climb_curves perform="TRUE"/>
			<climb_angle_curves perform="TRUE"/>
			<max_rate_of_climb_envelope perform="TRUE"/>
			<max_climb_angle_envelope perform="TRUE"/>
			<climb_time perform="TRUE"/>
			<fuel_used perform="TRUE"/>
		</climb>
		<cruise>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<cruise_grid perform="TRUE"/>
			<flight_envelope perform="TRUE"/>
		</cruise>
		<descent perform="TRUE"/>
		<landing>
			<simulations perform="TRUE"/>
		</landing>
		<payload_range perform="TRUE"/>
		<maneuvering_flight_and_gust_envelope perform="TRUE"/>
		<noise_trajectories>
			<take_off perform="TRUE"/>
			<landing perform="TRUE"/>
		</noise_trajectories>
		<mission_profile>
			<range_profile perform="TRUE"/>
			<time_profile perform="TRUE"/>
			<fuel_used_profile perform="TRUE"/>
			<weight_profile perform="TRUE"/>
			<emissions_profile perform="TRUE"/>
			<speed_profile perform="TRUE"/>
			<mach_proifle perform="TRUE"/>
			<cL_proifle perform="TRUE"/>
			<cD_proifle perform="TRUE"/>
			<efficiency_proifle perform="TRUE"/>
			<drag_thrust_proifle perform="TRUE"/>
			<fuel_flow_proifle perform="TRUE"/>
			<sfc_proifle perform="TRUE"/>
			<rate_of_climb_proifle perform="TRUE"/>
			<climb_angle_proifle perform="TRUE"/>
			<power_profile perform="TRUE"/>
			<energy_profile perform="TRUE"/>
		</mission_profile>
	</plot>
</jpad_config>
