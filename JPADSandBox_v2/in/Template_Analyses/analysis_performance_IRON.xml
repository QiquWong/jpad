<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <analysis>
        <performance
			id="JPAD Test analysis - PERFORMANCE"
			weights_from_xls_file="false" 
			file_weights="Weights.xlsx"
			balance_from_xls_file="false"
			file_balance="Balance.xlsx"
			aerodynamics_from_xls_file="false" 
			file_aerodynamics="Aerodynamics.xlsx"
			>
			<weights>
				<maximum_take_off_mass unit="kg">53610.0</maximum_take_off_mass>
				<operating_empty_mass unit="kg">33550.0</operating_empty_mass>
				<maximum_fuel_mass unit="kg">11959.0</maximum_fuel_mass>
				<single_passenger_mass unit="kg">106</single_passenger_mass>
			</weights>
			<balance>
				<xCG_max_aft unit="%">32</xCG_max_aft>
			</balance>
			<aerodynamics parabolicDragPolar="FALSE"> <!-- if TRUE uses a parabolic drag polar with wave drag correction; otherwise it reads the polar_curves tag -->
				<cLmax_clean_configuration>1.61</cLmax_clean_configuration>
				<cL_alpha_clean_configuration unit="1/deg">0.11</cL_alpha_clean_configuration>
				<cLmax_take_off_configuration>2.58</cLmax_take_off_configuration>
				<cL_alpha_take_off unit="1/deg">0.152</cL_alpha_take_off>
				<cL0_take_off_configuration>1.35</cL0_take_off_configuration>
				<cLmax_landing_configuration>3.0</cLmax_landing_configuration>
				<cL_alpha_landing unit="1/deg">0.1</cL_alpha_landing>
				<cL0_landing_configuration>1.8</cL0_landing_configuration>
				<!-- the following data are used only if parabolicDragPolar flag is set to TRUE. In any other case, they can be left blank -->
				<cD0></cD0>
				<oswald_cruise></oswald_cruise> 
				<oswald_climb></oswald_climb>
				<oswald_take_off></oswald_take_off>
				<oswald_landing></oswald_landing>
				<delta_CD0_flap_take_off></delta_CD0_flap_take_off>
				<delta_CD0_flap_landing></delta_CD0_flap_landing>
				<delta_CD0_landing_gears></delta_CD0_landing_gears>
				<delta_CD0_spoilers></delta_CD0_spoilers>
				<!---->
				<polar_curves>
					<cruise>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</cruise>
					<climb>
						<polar_curve_CL>0.074094656;0.164637869;0.255273202;0.34598965;0.436758067;0.527625136;0.618567896;0.709572936;0.800692941;0.891893729;0.983180144;1.074585696;1.166061602;1.25823474;1.351480396;1.442135025;1.526404611;1.600303194;1.659886007;1.701362773;1.720911295</polar_curve_CL>
						<polar_curve_CD>0.025938242;0.02574636;0.026068839;0.027095231;0.028945139;0.031597907;0.035038569;0.039358494;0.044328035;0.04988608;0.056107253;0.062838857;0.070087492;0.077918415;0.086506752;0.095604698;0.10486729;0.113938782;0.122842759;0.13199229;0.138327958</polar_curve_CD>
					</climb>
					<take_off>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</take_off>
					<landing>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</landing>
				</polar_curves>
			</aerodynamics>
			<!-- the following data can be assigned between tags, or not. If not, default values are used -->
			<takeoff_landing>
				<!-- general data -->
				<wind_speed_along_runway unit="m/s"></wind_speed_along_runway> <!-- positive if behind the aircraft (default = 0m/s)-->
					<!-- here is possible to assign the fricition coefficient function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_function>
					<friction_coefficient>0.025</friction_coefficient>
					<speed unit="m/s"></speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_function>
					<!-- here is possible to assign the fricition coefficient (with brakes) function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_with_brakes_function>
					<friction_coefficient_with_brakes>0.3</friction_coefficient_with_brakes>
					<speed unit="m/s"></speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_with_brakes_function>
				<!-- take-off data -->
				<dt_rotation unit="s">2</dt_rotation> <!-- the duration of the rotation phase (default = 3s) -->
				<dt_hold unit="s"></dt_hold> <!-- the duration of the constant CL phase (default = 0.5s) -->
				<alpha_ground unit="deg"></alpha_ground> <!-- angle of attack in BRF along the runway (default = 0Â°)-->
				<obstacle_take_off unit="ft"></obstacle_take_off> <!-- default = 35 ft -->
				<k_rotation>1.03</k_rotation> <!-- V_rotation/V_stall in take-off (default = 1.05) -->
				<alpha_dot_rotation>3</alpha_dot_rotation> <!-- rotation angular velocity (must be in deg/s) (default = 3.0) -->
				<k_cLmax></k_cLmax> <!-- percentage of the CLmax,TO not to be surpassed during the rotation  (default = 0.9)-->
				<drag_due_to_engine_failure>0.005</drag_due_to_engine_failure> <!-- drag coefficient to be added due to the failed engine and rudder deflection (default = 0.0) -->
				<k_alpha_dot></k_alpha_dot> <!-- indicates how fast the rotation maneuver is - faster with decreasing k_alpha_dot - (default 0.04) -->
				<!-- landing data -->
				<k_landing_weight></k_landing_weight> <!-- percentage of the maximum take-off weight to be used for the landing performance - (default = 0.97) -->
				<obstacle_landing unit="ft"></obstacle_landing> <!-- default = 50ft -->
				<theta_approach unit="deg"></theta_approach> <!-- default = 3deg -->
				<k_approach></k_approach> <!-- V_approach/V_stall in landing (default = 1.3) -->
				<k_flare></k_flare> <!-- V_flare/V_stall in landing (default = 1.23) -->
				<k_touch_down></k_touch_down> <!-- V_touch_down/V_stall in landing (default = 1.15) -->
				<free_roll_duration unit="s"></free_roll_duration> <!-- default = 2s -->
			</takeoff_landing>
			<climb>
				<k_climb_weight_AOE></k_climb_weight_AOE> <!-- percentage of the maximum take-off weight to be used for the climb performance (AOE) - (default = 1.0) -->
				<k_climb_weight_OEI></k_climb_weight_OEI> <!-- percentage of the maximum take-off weight to be used for the climb performance (OEI) - (default = 0.97) -->
				<climb_speed_CAS unit="kn">190</climb_speed_CAS>
			</climb>
			<cruise>
				<altitudes_array unit="ft">20000;25000;30000</altitudes_array>
				<k_cruise_weight></k_cruise_weight> <!-- percentage of the maximum take-off weight to be used for the cruise performance - (default = 0.97) -->
			</cruise>
			<descent>
				<rate_of_descent unit="ft/min">1500</rate_of_descent>
				<descent_speed_CAS unit="kn">220</descent_speed_CAS>
				<k_descent_weight></k_descent_weight> <!-- percentage of the maximum take-off weight to be used for the descent performance - (default = 0.9) -->
			</descent>
			<flight_maneuvering_and_gust_envelope>
				<cLmax_inverted></cLmax_inverted> <!-- to be assigned for the manoeuvring flight and gust envelope (default = -1.0) --> 
			</flight_maneuvering_and_gust_envelope>
			<mission_profile_and_payload_range>
			<!-- if alternate and holding phases have not to be taken into account, leave their tags empty -->
				<mission_range unit="nmi">1600</mission_range> 
				<alternate_cruise_length unit="nmi">100</alternate_cruise_length> <!-- (default = 0.0) -->
				<alternate_cruise_altitude unit="ft">15000</alternate_cruise_altitude> <!-- (default = 15.24 = landing obstacle) -->
				<alternate_cruise_mach_number>0.524</alternate_cruise_mach_number>
				<holding_duration unit="min">30</holding_duration> <!-- (default = 0.0) -->
				<holding_altitude unit="ft">1500</holding_altitude> <!-- (default = 0.0) -->
				<holding_mach_number>0.3</holding_mach_number>
				<fuel_reserve>0.05</fuel_reserve> <!-- (default = 0.0) -->
				<cruise_length unit="nmi">1300</cruise_length> <!-- this is a first guess value which will be managed by the calculator iteratively -->
				<cruise_sfc_function>
					<sfc>0.47;0.435;0.429</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</cruise_sfc_function>
				<alternate_cruise_sfc_function>
					<sfc>0.46;0.422;0.415</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</alternate_cruise_sfc_function>
				<holding_sfc_function>
					<sfc>0.444;0.3285;0.3047</sfc>
					<throttle>0.4;0.75;0.95</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</holding_sfc_function>
				<initial_mission_fuel unit="kg">6475</initial_mission_fuel> <!-- this is a first guess value which will be managed by the calculator iteratively -->
				<take_off_mission_altitude unit="ft">5400.0</take_off_mission_altitude>
				<landing_ground_idle_fuel_flow unit="kg/min"></landing_ground_idle_fuel_flow> <!-- have to be in kg/min !! (default = 0.0) -->
			</mission_profile_and_payload_range>
        </performance>
    </analysis>
	<plot>
		<takeoff>
			<simulations perform="TRUE"/>
			<balanced_field_length perform="TRUE"/>
			<vMC perform="TRUE"/>
		</takeoff>
		<climb>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<rate_of_climb_curves perform="TRUE"/>
			<climb_angle_curves perform="TRUE"/>
			<max_rate_of_climb_envelope perform="TRUE"/>
			<max_climb_angle_envelope perform="TRUE"/>
			<climb_time perform="TRUE"/>
		</climb>
		<cruise>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<cruise_grid perform="TRUE"/>
			<flight_envelope perform="TRUE"/>
		</cruise>
		<descent perform="TRUE"/>
		<landing>
			<simulations perform="TRUE"/>
		</landing>
		<payload_range perform="TRUE"/>
		<maneuvering_flight_and_gust_envelope perform="TRUE"/>
		<mission_profile>
			<range_profile perform="TRUE"/>
			<time_profile perform="TRUE"/>
			<fuel_used_profile perform="TRUE"/>
			<weight_profile perform="TRUE"/>
		</mission_profile>
	</plot>
</jpad_config>
