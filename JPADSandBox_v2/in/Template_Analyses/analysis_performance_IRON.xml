				<alternate_cruise_length unit="nmi">100</alternate_cruise_length> <!-- (default = 0.0) -->
				<alternate_cruise_altitude unit="ft">15000</alternate_cruise_altitude> <!-- (default = 15.24 = landing obstacle) -->
				<holding_duration unit="min">30</holding_duration> <!-- (default = 0.0) -->
				<holding_altitude unit="ft">1500</holding_altitude> <!-- (default = 0.0) -->
				<holding_mach_number>0.3</holding_mach_number>
				<fuel_reserve>0.05</fuel_reserve> <!-- (default = 0.0) -->
				<cruise_length unit="nmi">1300</cruise_length> <!-- this is a first guess value which will be managed by the calculator iteratively -->
				<cruise_sfc_function>
					<sfc>0.517;0.479;0.472</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</cruise_sfc_function>
				<alternate_cruise_sfc_function>
					<sfc>0.5611396;0.510763;0.501787</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</alternate_cruise_sfc_function>
				<holding_sfc_function>
					<sfc>0.4884;0.36135;0.33517</sfc>
					<throttle>0.4;0.75;0.95</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</holding_sfc_function>
				<initial_mission_fuel unit="kg">6475</initial_mission_fuel> <!-- this is a first guess value which will be managed by the calculator iteratively -->
				<take_off_mission_altitude unit="ft">5400.0</take_off_mission_altitude>
				<landing_ground_idle_fuel_flow unit="kg/min"></landing_ground_idle_fuel_flow> <!-- have to be in kg/min !! (default = 0.0) -->
			</mission_profile_and_payload_range>
        </performance>
