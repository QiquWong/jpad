<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <analysis>
        <performance
			id="IRON - PERFORMANCE"
			weights_from_previous_file="false" 
			aerodynamics_from_previous_file="false" 
			>
			<weights>
				<!-- MTOM -->
				<maximum_take_off_mass unit="kg">55239</maximum_take_off_mass> <!-- IRON = 53610 / CS300 = 67585 -->
				<operating_empty_mass unit="kg">34679</operating_empty_mass> <!-- IRON = 33550 / CS300 = 37081 -->
				<maximum_fuel_mass unit="kg">10820</maximum_fuel_mass> <!-- IRON = 11959 / CS300 = 17213 -->
				<single_passenger_mass unit="kg">105</single_passenger_mass> 
			</weights>
			<aerodynamics> 
				<xcg value="-0.37" parabolicDragPolar="FALSE"> <!-- if TRUE uses a parabolic drag polar with wave drag correction; otherwise it reads the polar_curves tag -->
					<cLmax_clean_configuration>1.837</cLmax_clean_configuration>
					<cL_alpha_clean_configuration unit="1/deg">0.13345</cL_alpha_clean_configuration>
					<!-- CLalpha increased by 1.3 (ground effect) --> 
					<cLmax_take_off_configuration>2.46</cLmax_take_off_configuration>
					<cL_alpha_take_off unit="1/deg">0.12896</cL_alpha_take_off> 
					<cL0_take_off_configuration>1.4739</cL0_take_off_configuration>
					<!---->
					<cLmax_landing_configuration>3.184</cLmax_landing_configuration> <!-- value of the trimmed CLmax in landing used to determine the approach speed --> 
					<cL_alpha_landing unit="1/deg">0.13268</cL_alpha_landing> <!-- value of the CLalpha with ground effect (used during ground roll) -->
					<cL0_landing_configuration>2.024</cL0_landing_configuration> <!-- value of the CL0 with ground effect (used during ground roll) -->
					<!-- the following data are used only if parabolicDragPolar flag is set to TRUE. In any other case, they can be left blank -->
					<cD0></cD0>
					<oswald_cruise></oswald_cruise> 
					<oswald_climb></oswald_climb>
					<oswald_take_off></oswald_take_off>
					<oswald_landing></oswald_landing>
					<delta_CD0_flap_take_off></delta_CD0_flap_take_off>
					<delta_CD0_flap_landing></delta_CD0_flap_landing>
					<delta_CD0_landing_gears></delta_CD0_landing_gears>
					<delta_CD0_spoilers></delta_CD0_spoilers>
					<!---->
					<polar_curves>
						<cruise>
							<polar_curve_CL>-0.27357;-0.18704;-0.10042;-0.01373;0.07303;0.15987;0.24677;0.33374;0.42077;0.50787;0.59502;0.68223;0.76949;0.85680;0.94416;1.03157;1.11902;1.20651;1.29404;1.36858;1.40534</polar_curve_CL>
							<polar_curve_CD>0.03340;0.03089;0.02894;0.02754;0.02671;0.02643;0.02672;0.02757;0.02900;0.03100;0.03357;0.03672;0.04046;0.04478;0.04968;0.05517;0.06124;0.06791;0.07517;0.08172;0.08482</polar_curve_CD>
						</cruise>
						<climb>
							<polar_curve_CL>-0.25658;-0.13736;-0.01795;0.10161;0.22134;0.34121;0.46122;0.58135;0.70161;0.82196;0.94242;1.06301;1.18363;1.30427;1.42502;1.54581;1.66665;1.78750;1.83725</polar_curve_CL>
							<polar_curve_CD>0.03094;0.02799;0.02606;0.02515;0.02528;0.02645;0.02868;0.03197;0.03632;0.04176;0.04827;0.05587;0.06456;0.07434;0.08521;0.09719;0.11026;0.12443;0.12545</polar_curve_CD>
						</climb>
						<take_off>
							<polar_curve_CL>0.718017454;0.837244473;0.956447966;1.075828966;1.19537771;1.315084414;1.434939274;1.554932468;1.675054155;1.795294475;1.915643548;2.036132567;2.156669438;2.277244224;2.397929157;2.459524655</polar_curve_CL>
							<polar_curve_CD>0.07424;0.07359;0.07354;0.07411;0.07528;0.07707;0.07947;0.08249;0.08612;0.09037;0.09524;0.10072;0.10682;0.11353;0.12087;0.134422</polar_curve_CD>
						</take_off>
						<landing>
							<polar_curve_CL>1.263905018;1.383132038;1.502136307;1.621329623;1.740702207;1.860244266;1.979945985;2.099797536;2.219789069;2.339910721;2.46015261;2.58054593;2.700998585;2.821500646;2.942124346;3.062818635;3.183573534</polar_curve_CL>
							<polar_curve_CD>0.09776;0.09694;0.09673;0.09713;0.09814;0.09976;0.10200;0.10485;0.10831;0.11239;0.11709;0.12240;0.12833;0.13488;0.14204;0.14983;0.15823</polar_curve_CD>
						</landing>
					</polar_curves>
					<tau_rudder_function calculate="FALSE">
						<tau>0.0000;0.5359;0.5648;0.5502;0.5261</tau>
						<delta_rudder unit="deg">0.0;10.0;20.0;25.0;30.0</delta_rudder> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_rudder_function>
				</xcg>
			</aerodynamics>
			<!-- the following data can be assigned between tags, or not. If not, default values are used -->
			<takeoff_landing>
				<!-- general data -->
				<wind_speed_along_runway unit="m/s"></wind_speed_along_runway> <!-- positive if behind the aircraft (default = 0m/s)-->
					<!-- here is possible to assign the fricition coefficient function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_function>
					<friction_coefficient>0.025</friction_coefficient>
					<speed unit="m/s"></speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_function>
					<!-- here is possible to assign the fricition coefficient (with brakes) function of the speed. If only one value is given, the function is constant -->
				<wheels_friction_coefficient_with_brakes_function>
					<friction_coefficient_with_brakes>0.94;0.89;0.85;0.8;0.74;0.68;0.62;0.57;0.52</friction_coefficient_with_brakes>
					<speed unit="m/s">0;10;20;30;40;50;60;70;80</speed> <!-- if the friction coefficient array contains only 1 value this tag can be left empty -->
				</wheels_friction_coefficient_with_brakes_function>
				<!-- take-off data -->
				<dt_rotation unit="s">2</dt_rotation> <!-- the duration of the rotation phase (default = 3s) -->
				<dt_hold unit="s"></dt_hold> <!-- the duration of the constant CL phase (default = 0.5s) -->
				<alpha_ground unit="deg"></alpha_ground> <!-- angle of attack in BRF along the runway (default = 0Â°)-->
				<obstacle_take_off unit="ft"></obstacle_take_off> <!-- default = 35 ft -->
				<k_rotation></k_rotation> <!-- V_rotation/V_stall in take-off (default = 1.05) -->
				<alpha_dot_rotation>4</alpha_dot_rotation> <!-- rotation angular velocity (must be in deg/s) (default = 3.0) -->
				<k_cLmax></k_cLmax> <!-- percentage of the CLmax,TO not to be surpassed during the rotation  (default = 0.9)-->
				<drag_due_to_engine_failure>0.005</drag_due_to_engine_failure> <!-- drag coefficient to be added due to the failed engine and rudder deflection (default = 0.0) -->
				<k_alpha_dot></k_alpha_dot> <!-- indicates how fast the rotation maneuver is - faster with decreasing k_alpha_dot - (default 0.04) -->
				<!-- landing data -->
				<k_landing_weight></k_landing_weight> <!-- percentage of the maximum take-off weight to be used for the landing performance - (default = 0.97) -->
				<obstacle_landing unit="ft"></obstacle_landing> <!-- default = 50ft -->
				<theta_approach unit="deg"></theta_approach> <!-- default = 3deg -->
				<k_approach></k_approach> <!-- V_approach/V_stall in landing (default = 1.3) -->
				<k_flare></k_flare> <!-- V_flare/V_stall in landing (default = 1.23) -->
				<k_touch_down></k_touch_down> <!-- V_touch_down/V_stall in landing (default = 1.15) -->
				<free_roll_duration unit="s"></free_roll_duration> <!-- default = 2s -->
			</takeoff_landing>
			<climb>
			<!-- CLIMB TO CRUISE -->
				<k_climb_weight_AEO></k_climb_weight_AEO> <!-- percentage of the maximum take-off weight to be used for the climb performance (AOE) - (default = 1.0) -->
				<k_climb_weight_OEI></k_climb_weight_OEI> <!-- percentage of the maximum take-off weight to be used for the climb performance (OEI) - (default = 0.97) -->
				<initial_climb_altitude unit="ft">0.0</initial_climb_altitude>
				<final_climb_altitude unit="ft">25000.0</final_climb_altitude>
				<climb_speed_CAS unit="kn">190</climb_speed_CAS>
			<!-- SECOND SEGMENT TAKE-OFF -->
			<!--
				<k_climb_weight_AEO></k_climb_weight_AEO>
				<k_climb_weight_OEI></k_climb_weight_OEI>
				<initial_climb_altitude unit="ft">0.0</initial_climb_altitude>
				<final_climb_altitude unit="ft">25000</final_climb_altitude>
				<climb_speed_CAS unit="kn">190</climb_speed_CAS>
			-->
			</climb>
			<cruise>
				<altitudes_array unit="ft">20000;25000;30000</altitudes_array>
				<k_cruise_weight></k_cruise_weight> <!-- percentage of the maximum take-off weight to be used for the cruise performance - (default = 0.97) -->
			</cruise>
			<descent>
				<rate_of_descent unit="ft/min">1500</rate_of_descent>
				<descent_speed_CAS unit="kn">220</descent_speed_CAS>
				<k_descent_weight></k_descent_weight> <!-- percentage of the maximum take-off weight to be used for the descent performance - (default = 0.9) -->
				<initial_descent_altitude unit="ft">30000</initial_descent_altitude>
				<final_descent_altitude unit="ft">1500.0</final_descent_altitude>
			</descent>
			<flight_maneuvering_and_gust_envelope>
				<cLmax_inverted></cLmax_inverted> <!-- to be assigned for the manoeuvring flight and gust envelope (default = -1.0) --> 
			</flight_maneuvering_and_gust_envelope>
			<mission_profile_and_payload_range>
			<!-- if alternate and holding phases have not to be taken into account, leave their tags empty -->
				<mission_range unit="nmi">400</mission_range> 
				<alternate_cruise_length unit="nmi">100</alternate_cruise_length> <!-- (default = 0.0) -->
				<alternate_cruise_altitude unit="ft">15000</alternate_cruise_altitude> <!-- (default = 15.24 = landing obstacle) -->
				<holding_duration unit="min">30</holding_duration> <!-- (default = 0.0) -->
				<holding_altitude unit="ft">1500</holding_altitude> <!-- (default = 0.0) -->
				<holding_mach_number>0.3</holding_mach_number>
				<fuel_reserve>0.05</fuel_reserve> <!-- (default = 0.0) -->
				<cruise_sfc_function calculate="FALSE">
					<sfc>0.47;0.435;0.429</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty (THROTTLE COMPARISON CS300 = 1.035;1.311;1.38)-->
				</cruise_sfc_function>
				<alternate_cruise_sfc_function calculate="FALSE">
					<sfc>0.51036;0.46433;0.45617</sfc>
					<throttle>0.75;0.95;1.0</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty (THROTTLE COMPARISON CS300 = 1.035;1.311;1.38)-->
				</alternate_cruise_sfc_function>
				<holding_sfc_function calculate="FALSE">
					<sfc>0.444;0.3285;0.3047</sfc>
					<throttle>0.4;0.75;0.95</throttle> <!-- if the friction coefficient array contains only 1 value this tag can be left empty (THROTTLE COMPARISON CS300 = 0.552;1.035;1.3118)-->
				</holding_sfc_function>
				<take_off_mission_altitude unit="ft">5400.0</take_off_mission_altitude>
				<landing_ground_idle_fuel_flow unit="kg/min"></landing_ground_idle_fuel_flow> <!-- have to be in kg/min !! (default = 0.0) -->
			</mission_profile_and_payload_range>
        </performance>
    </analysis>
	<plot>
		<takeoff>
			<simulations perform="TRUE"/>
			<balanced_field_length perform="TRUE"/>
			<vMC perform="TRUE"/>
		</takeoff>
		<climb>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<rate_of_climb_curves perform="TRUE"/>
			<climb_angle_curves perform="TRUE"/>
			<max_rate_of_climb_envelope perform="TRUE"/>
			<max_climb_angle_envelope perform="TRUE"/>
			<climb_time perform="TRUE"/>
			<fuel_used perform="TRUE"/>
		</climb>
		<cruise>
			<thrust_drag perform="TRUE"/>
			<power_needed_and_available perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<cruise_grid perform="TRUE"/>
			<flight_envelope perform="TRUE"/>
		</cruise>
		<descent perform="TRUE"/>
		<landing>
			<simulations perform="TRUE"/>
		</landing>
		<payload_range perform="TRUE"/>
		<maneuvering_flight_and_gust_envelope perform="TRUE"/>
		<mission_profile>
			<range_profile perform="TRUE"/>
			<time_profile perform="TRUE"/>
			<fuel_used_profile perform="TRUE"/>
			<weight_profile perform="TRUE"/>
		</mission_profile>
	</plot>
</jpad_config>
