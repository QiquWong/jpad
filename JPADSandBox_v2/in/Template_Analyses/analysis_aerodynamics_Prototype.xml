                    </cM_vs_alpha>
					<cM_at_alpha_current perform="TRUE" method="SEMIEMPIRICAL"/>
				</pitching_moment>
			</nacelles_analyses>
			<aircraft_analyses>
				<!-- <downwash perform="TRUE" downwash_gradient="CONSTANT" method="SLINGERLAND"/> -->
                <downwash perform="FALSE" downwash_gradient="INPUT" method="UNKNOWN"/>
