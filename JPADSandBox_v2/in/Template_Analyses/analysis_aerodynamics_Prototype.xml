                        <downwash_gradient>0.2;0.3;0.7;0.9;0.95;0.9;0.85;0.7</downwash_gradient>
                    </downwash_gradient_vs_alpha>
                </downwash/>
				<dynamic_pressure_ratio perform="TRUE" method="SEMIEMPIRICAL"/>
				<buffet_barrier perform="FALSE"/> <!-- TODO: IMPLEMENT THIS (work in progress) -->
				<lift_curve perform="TRUE" fuselage_effects="TRUE"/>
				<drag_polar perform="TRUE" delta_CD0_miscellaneous="0.0"/>
				<moment_curve perform="TRUE"/>
				<trimmed_lift_curve perform="TRUE"/>
				<trimmed_drag_polar perform="TRUE"/>
				<trimmed_moment_curve perform="TRUE"/>
				<longitudinal_static_stability perform="TRUE" wing_pendular_stability_effect="TRUE"/>
				<lateral_static_stability perform="TRUE"/>
				<directional_static_stability perform="TRUE"/>
				<dynamic_stability perform="TRUE"/>
			</aircraft_analyses>
        </aerodynamics_and_stability>
    </analysis>
