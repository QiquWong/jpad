			</vertical_tail_analyses>
			<fuselage_analyses>
				<drag>
					<cD0_upsweep perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_windshield perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_base perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_parasite perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_total perform="TRUE" method="FUSDES"/>
					<cD_induced perform="TRUE" method="FUSDES"/>
					<!-- <cD_vs_cL perform="TRUE" method="FUSDES"/> -->
                    <cD_vs_cL perform="FALSE" method="SEMIEMPIRICAL">
                        <cL>0.2;0.3;0.7;0.9;0.95;0.09;0.085;0.07</cL>
                        <cD>0.02;0.03;0.07;0.09;0.095;0.09;0.085;0.07</cD>
                    </cD_vs_cL>
					<cD_at_alpha_current perform="TRUE" method="FUSDES"/>
				</drag>
				<pitching_moment>
					<cM0 perform="TRUE" method="FUSDES"/>
					<cM_alpha perform="TRUE" method="FUSDES"/>
					<!-- <cM_vs_alpha perform="TRUE" method="FUSDES"/> -->
                    <cM_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cM>0.2;0.3;0.7;0.9;0.95;0.9;0.85;0.7</cM>
                    </cM_vs_alpha>
					<cM_at_alpha_current perform="TRUE" method="FUSDES"/>
				</pitching_moment>
			</fuselage_analyses>
			<nacelles_analyses>
				<drag>
					<cD0_base perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_parasite perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD0_total perform="TRUE" method="SEMIEMPIRICAL"/>
					<cD_induced perform="TRUE" method="SEMIEMPIRICAL"/>
					<!-- <cD_vs_cL perform="TRUE" method="SEMIEMPIRICAL"/> -->
