<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
	<!-- 
		plot=TRUE -> enters the plot tag.
		plot=FALSE -> plot nothing.
		
		All the variables which have "METHOD" as attirubte, can be calculated with the deifned method (TODO: or can be assigned)
		
		IF THE MOMENTIUM POLE IS NOT ASSIGNED (wing/HTail/VTail), IT WILL BE ASSUMED AS THE Xac CALCUALTED USING THE METHOD SPECIFIED HERE 
		(IF THE CALCULATION IS NOT REQUIRED A DEFAULT METHOD IS ASSUMED EQUAL TO DE_YOUNG_HARPER)
		
	-->
    <analysis>
        <aerodynamics_and_stability 
			id="JPAD Test analysis - AERODYNAMICS PROTOTYPE"
			balance_from_xls_file="false"
			file_balance="Balance.xlsx"
			>
			<balance>
			<!-- 
			These values will be read only if the "balance_from_xls_file" is FALSE. 
			Otherwise the all the CG positions in every condition will be read from the Balance output file.
			-->
				<!-- the values of the centre of gravity position are referred to the mean aerodynamic chord of the wing -->
				<adimensional_center_of_gravity_x_position_list>0.15:0.25;0.33</adimensional_center_of_gravity_x_position_list>
				<adimensional_center_of_gravity_z_position_list>0.0;0.0;0.0</adimensional_center_of_gravity_z_position_list>
				<rear_landing_gear_dimensional_center_of_gravity_z_position>-3.067</rear_landing_gear_dimensional_center_of_gravity_z_position>
			</balance>
			<global_data dynamic_pressure_ratio_from_file="FALSE" tau_elevator_from_file="FALSE" tau_rudder_from_file="FALSE">
				<alpha_body_initial unit="deg">-10</alpha_body_initial>
				<alpha_body_final unit="deg">23</alpha_body_final>
				<number_of_alpha_body>34</number_of_alpha_body>
				<beta_initial unit="deg">0</beta_initial>
				<beta_final unit="deg">20</beta_final>
				<number_of_beta>21</number_of_beta>
				<alpha_wing_array_for_distributions unit="deg">0.0;2.0;10.0</alpha_wing_array_for_distributions> <!-- these distributions are only in CLEAN configuration -->
				<alpha_horizontal_tail_array_for_distributions unit="deg">0.0;2.0;10.0</alpha_horizontal_tail_array_for_distributions>
				<beta_vertical_tail_array_for_distributions unit="deg">0.0;2.0;10.0</beta_vertical_tail_array_for_distributions>
				<number_of_points_semispawise_wing>50</number_of_points_semispawise_wing>
				<number_of_points_semispawise_horizontal_tail>50</number_of_points_semispawise_horizontal_tail>
				<number_of_points_semispawise_vertical_tail>50</number_of_points_semispawise_vertical_tail>
				<delta_elevator_array unit="deg">0.0;-5.0;-10.0;-15.0;-20.0;-25.0</delta_elevator_array>
				<delta_rudder_array unit="deg">0.0;5.0;10.0;15.0;20.0;25.0</delta_rudder_array>
				<adimensional_wing_momentum_pole>0.25</adimensional_wing_momentum_pole> 
				<adimensional_horizontal_tail_momentum_pole>0.25</adimensional_horizontal_tail_momentum_pole> 
				<adimensional_vertical_tail_momentum_pole>0.25</adimensional_vertical_tail_momentum_pole> 
				<adimensional_fuselage_momentum_pole>0.5</adimensional_fuselage_momentum_pole> <!-- adimensional: referred to fuselage length -->
				<!-- optional values which are read only if the related attribute in "global_data" is "TRUE" --> 
				<horizontal_tail_dynamic_pressure_ratio>0.8</horizontal_tail_dynamic_pressure_ratio>
				<tau_elevator_function>
					<tau>0.025</tau>
					<delta_elevator unit="deg"></delta_elevator> <!-- if the tau array contains only 1 value this tag can be left empty -->
				</tau_elevator_function>
				<tau_rudder_function>
					<tau>0.025</tau>
					<delta_rudder unit="deg"></delta_rudder> <!-- if the tau array contains only 1 value this tag can be left empty -->
				</tau_rudder_function>	
				<!---->
			</global_data>
			<wing_analyses>
				<critical_mach perform="TRUE" method="KROO"></critical_mach>
				<aerodynamic_center perform="TRUE" method="DEYOUNG_HARPER"></aerodynamic_center> <!-- referred to the mean aerodynamic chord --> 
				<lift>
					<cL_alpha perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_zero perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_star perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_max perform="TRUE" method="NASA_BLACKWELL"/>
					<alpha_zero_lift perform="TRUE" method="INTEGRAL_MEAN_TWIST"/>
					<alpha_star perform="TRUE" method="MEAN_AIRFOIL_INFLUENCE_AREAS"/>
					<alpha_stall perform="TRUE" method="NASA_BLACKWELL"/>
					<!-- <cL_vs_alpha perform="TRUE" method="NASA_BLACKWELL"/> -->
                    <cL_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cL>0.0;0.1;0.5;0.7;0.75;0.7;0.65;0.5</cL>
                    </cL_vs_alpha>
					<lift_distributions perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_at_alpha_current perform="TRUE" method="NASA_BLACKWELL"/>
				</lift>
				<high_lift_devices>
					<high_lift_devices_effects perform="TRUE" method="SEMIEMPIRICAL"/>
					<!-- <cL_vs_alpha perform="TRUE" method="SEMIEMPIRICAL"/> -->
                    <cL_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cL>0.2;0.3;0.7;0.9;0.95;0.9;0.85;0.7</cL>
                    </cL_vs_alpha>
					<cL_at_alpha_current perform="TRUE"/>
					<cD_at_alpha_current perform="TRUE"/>
					<cm_at_alpha_current perform="TRUE"/>
				</high_lift_devices>
				<drag>
					<cD_Zero perform="TRUE" method="SEMIEMPIRICAL"/>
					<oswald_factor perform="TRUE" method="RAYMER"/>
					<cD_induced perform="TRUE" method="RAYMER"/>
					<cD_wave perform="TRUE" method="LOCK_KORN_WITH_KROO"/>
					<!-- <cD_vs_cL perform="TRUE" method="AIRFOIL_DISTRIBUTION"/> -->
                    <cD_vs_cL perform="FALSE" method="SEMIEMPIRICAL">
                        <cL>0.2;0.3;0.7;0.9;0.95;0.09;0.085;0.07</cL>
                        <cD>0.02;0.03;0.07;0.09;0.095;0.09;0.085;0.07</cD>
                    </cD_vs_cL>
					<drag_distributions perform="TRUE" method="NASA_BLACKWELL"/>
					<cD_at_alpha_current perform="TRUE" method="AIRFOIL_DISTRIBUTION"/>
				</drag>
				<pitching_moment>
					<cM_ac perform="TRUE" method="AIRFOIL_DISTRIBUTION"/>
					<cM_alpha perform="TRUE" method="POLHAMUS"/>
					<!-- <cM_vs_alpha perform="TRUE" method="AIRFOIL_DISTRIBUTION"/> -->
                    <cM_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cM>0.2;0.3;0.7;0.9;0.95;0.9;0.85;0.7</cM>
                    </cM_vs_alpha>
					<cM_at_alpha_current perform="TRUE" method="AIRFOIL_DISTRIBUTION"/>
					<moment_distributions perform="TRUE" method="AIRFOIL_DISTRIBUTION"/>
				</pitching_moment>
			</wing_analyses>
			<horizontal_tail_analyses>
				<critical_mach perform="TRUE" method="KROO"></critical_mach>
				<aerodynamic_center perform="TRUE" method="DEYOUNG_HARPER"></aerodynamic_center> <!-- referred to the mean aerodynamic chord --> 
				<lift>
					<cL_alpha perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_zero perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_star perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_max perform="TRUE" method="NASA_BLACKWELL"/>
					<alpha_zero_lift perform="TRUE" method="INTEGRAL_MEAN_TWIST"/>
					<alpha_star perform="TRUE" method="INTEGRAL_MEAN_TWIST"/>
					<alpha_stall perform="TRUE" method="NASA_BLACKWELL"/>
					<!-- <cL_vs_alpha perform="TRUE" method="NASA_BLACKWELL"/> -->
					<cL_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cL>0.0;0.1;0.5;0.7;0.75;0.7;0.65;0.5</cL>
                    </cL_vs_alpha>
					<lift_distributions perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_at_alpha_current perform="TRUE" method="NASA_BLACKWELL"/>
				</lift>
				<elevator>
					<high_lift_devices_effects perform="TRUE" method="SEMIEMPIRICAL"/>
					<!-- <cL_vs_alpha perform="TRUE" method="SEMIEMPIRICAL"/> -->
                    <cL_vs_alpha perform="FALSE" method="SEMIEMPIRICAL">
                        <alpha unit="deg">-3.0;0.0;8.0;11.0;12.0;13.0;14.0;15.0</alpha>
                        <cL>0.2;0.3;0.7;0.9;0.95;0.9;0.85;0.7</cL>
                    </cL_vs_alpha>
					<cL_at_alpha_current perform="TRUE"/>
					<cD_at_alpha_current perform="TRUE"/>
					<cm_at_alpha_current perform="TRUE"/>
				</elevator>
				<drag>
					<cD_Zero perform="TRUE" method="AIRFOIL_DISTRIBUTION"/>
					<oswald_factor perform="TRUE" method="RAYMER"/>
					<cD_induced perform="TRUE" method="RAYMER"/>
					<cD_wave perform="TRUE" method="LOCK_KORN_WITH_KROO"/>
					<!-- <cD_vs_cL perform="TRUE" method="AIRFOIL_DISTRIBUTION"/> -->
                    <cD_vs_cL perform="FALSE" method="SEMIEMPIRICAL">
                        <cL>0.2;0.3;0.7;0.9;0.95;0.09;0.085;0.07</cL>
                        <cD>0.02;0.03;0.07;0.09;0.095;0.09;0.085;0.07</cD>
                    </cD_vs_cL>
					<drag_distributions perform="TRUE" method="NASA_BLACKWELL"/>
