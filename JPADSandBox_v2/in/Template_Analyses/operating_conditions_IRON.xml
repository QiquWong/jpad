<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
	<operating_conditions id="JPAD Operating Conditions">
		<global_data>
			<!-- angle of attack at which the user wants to evaluate a generic aerodynamic output -->
			<alpha_current unit="deg">2</alpha_current> <!-- default = 2.0 deg -->
			<!-- array of angles of attack to be used for the the aerodynamics distributions (max 7 values) -->
			<alpha_array unit="deg">2;6;10;14;18;22;24</alpha_array> <!-- default = [2,6,10,14,18,22,24] -->
		</global_data>
		<climb>
			<mach>0.3</mach>
			<altitude_to_reach unit="ft">25000</altitude_to_reach>
		</climb>
		<cruise>
			<mach>0.585</mach>	
			<altitude unit="ft">30000</altitude>
			<throttle>1.0</throttle>
		</cruise>
		<take_off>
			<mach>0.172</mach> <!-- default = 0.2 -->
			<altitude unit="m">0.0</altitude> <!-- default = 0.0 -->
			<throttle></throttle> <!-- default = 1.0 -->
			<delta_flap unit="deg">15;15</delta_flap>
			<delta_slat unit="deg"></delta_slat>
		</take_off>
		<landing>
			<mach></mach> <!-- default = 0.2 -->
			<altitude unit="m"></altitude> <!-- default = 0.0 -->
			<reverse_throttle>0.0059</reverse_throttle> <!-- default = 0.0 -->
			<delta_flap unit="deg">40;40</delta_flap>
			<delta_slat unit="deg"></delta_slat>
		</landing>
	</operating_conditions>
</jpad_config>






