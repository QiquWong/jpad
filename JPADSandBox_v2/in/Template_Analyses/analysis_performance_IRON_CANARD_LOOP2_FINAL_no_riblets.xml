					<delta_CD0_flap_landing></delta_CD0_flap_landing>
					<delta_CD0_landing_gears></delta_CD0_landing_gears>
					<delta_CD0_spoilers></delta_CD0_spoilers>
					<!---->
					<polar_curves>
						<cruise> <!-- xc/c -0.22 -->
							<polar_curve_CL>-0.01993;0.05248;0.12489;0.19729;0.26970;0.34211;0.41452;0.48693;0.55933;0.63174;0.70415;0.77656;0.84897;0.92137;0.99378;1.06619;1.13860;1.21101;1.28341;1.35582;1.42823</polar_curve_CL>
							<polar_curve_CD>0.02519;0.02487;0.02487;0.02520;0.02585;0.02683;0.02813;0.02975;0.03170;0.03397;0.03657;0.03949;0.04273;0.04630;0.05019;0.05441;0.05895;0.06381;0.06900;0.07452;0.08035</polar_curve_CD>
						</cruise>
						<climb> <!-- xc/c -0.22 -->
							<polar_curve_CL>0.426967852;0.48495239;0.542936928;0.600921467;0.658906005;0.716890544;0.774875082;0.832859621;0.890844159;0.948828697;1.006813236;1.064797774;1.122782313;1.180766851;1.23875139;1.296735928;1.354720466;1.412705005;1.470689543;1.528674082;1.58665862</polar_curve_CL>
							<polar_curve_CD>0.033341261;0.034752388;0.036348094;0.038128378;0.040093241;0.042242682;0.044576702;0.0470953;0.049798476;0.052686232;0.055758565;0.059015477;0.062456968;0.066083037;0.069893685;0.073888911;0.078068715;0.082433098;0.08698206;0.0917156;0.096633718</polar_curve_CD>									
