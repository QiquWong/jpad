					<cLmax_clean_configuration>1.85</cLmax_clean_configuration>
					<cL_alpha_clean_configuration unit="1/°">0.116</cL_alpha_clean_configuration>
					<cLmax_take_off_configuration>2.85</cLmax_take_off_configuration> <!-- to be updated -->
					<cL_alpha_take_off unit="1/°">0.1118</cL_alpha_take_off> <!-- to be updated --> 
					<cL0_take_off_configuration>1.5014</cL0_take_off_configuration> <!-- to be updated -->
					<!---->
					<cLmax_landing_configuration>3.13</cLmax_landing_configuration>
					<cL_alpha_landing unit="1/°">0.0888</cL_alpha_landing>
					<cL0_landing_configuration>1.866</cL0_landing_configuration>
					<!-- the following data are used only if parabolicDragPolar flag is set to TRUE. In any other case, they can be left blank -->
					<cD0></cD0>
					<oswald_cruise></oswald_cruise> 
					<oswald_climb></oswald_climb>
					<oswald_take_off></oswald_take_off>
					<oswald_landing></oswald_landing>
