<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <analysis>
        <performance
			id="IRON LOOP 2 - PERFORMANCE (NO RIBLETS)"
			weights_from_previous_file="false" 
			aerodynamics_from_previous_file="false" 
			>
			<weights>
				<!-- MTOM -->
				<maximum_take_off_mass unit="kg">55174</maximum_take_off_mass>
				<operating_empty_mass unit="kg">34167</operating_empty_mass> 
				<maximum_fuel_mass unit="kg">7252</maximum_fuel_mass> 
				<single_passenger_mass unit="kg">108</single_passenger_mass> 
			</weights>
			<aerodynamics> 
				<xcg value="0.0" parabolicDragPolar="FALSE"> <!-- if TRUE uses a parabolic drag polar with wave drag correction; otherwise it reads the polar_curves tag -->
