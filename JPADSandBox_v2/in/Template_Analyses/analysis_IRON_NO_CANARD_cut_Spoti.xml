			climb_condition="FALSE"
			file_climb_condition="analysis_aerodynamics_Prototype_LOOP1_updated_climb.xml"
			cruise_condition="FALSE"
			file_cruise_condition="analysis_aerodynamics_Prototype_LOOP1_updated_cruise.xml"
			landing_condition="FALSE"
			file_landing_condition="analysis_aerodynamics_Prototype_LOOP1_updated_landing.xml"
	/>		
    </analyses>
</jpad_config>
