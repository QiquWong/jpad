<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
	<!-- 
		plot=TRUE -> enters the plot tag.
		plot=FALSE -> plot nothing.
		
		All the variables which have "METHOD" as attirubte, can be calculated with the deifned method or can be assigned filling the value if the METHOD is equal to "INPUT"
		
		Each condition in "global_data" will be read only if the related flag in the analysis file is set to TRUE !!
	-->
    <analysis>
        <aerodynamics_and_stability 
			id="JPAD Test analysis - AERODYNAMICS"
			balance_from_xls_file="false"
			file_balance="Balance.xlsx"
			>
			<balance>
			<!-- 
			These values will be read only if the "balance_from_xls_file" is FALSE. 
			Otherwise the all the CG positions in every condition will be read from the Balance output file.
			-->
				<take_off_condition>
					<!-- the values of the centre of gravity position are referred to the mean aerodynamic chord of the wing -->
					<adimensional_centre_of_gravity_x_position_list></adimensional_centre_of_gravity_x_position_list>
					<adimensional_centre_of_gravity_z_position_list></adimensional_centre_of_gravity_z_position_list>
				</take_off_condition>					
				<climb_condition>
					<!-- the values of the centre of gravity position are referred to the mean aerodynamic chord of the wing -->
					<adimensional_centre_of_gravity_x_position_list></adimensional_centre_of_gravity_x_position_list>
					<adimensional_centre_of_gravity_z_position_list></adimensional_centre_of_gravity_z_position_list>
				</climb_condition>					
				<cruise_condition>
					<!-- the values of the centre of gravity position are referred to the mean aerodynamic chord of the wing -->
					<adimensional_centre_of_gravity_x_position_list></adimensional_centre_of_gravity_x_position_list>
					<adimensional_centre_of_gravity_z_position_list></adimensional_centre_of_gravity_z_position_list>
				</cruise_condition>	
				<landing_condition>
					<!-- the values of the centre of gravity position are referred to the mean aerodynamic chord of the wing -->
					<adimensional_centre_of_gravity_x_position_list></adimensional_centre_of_gravity_x_position_list>
					<adimensional_centre_of_gravity_z_position_list></adimensional_centre_of_gravity_z_position_list>
				</landing_condition>
			</balance>
			<global_data dynamic_pressure_ratio_from_file="FALSE" tau_elevator_from_file="FALSE">
				<take_off_condition>
					<alpha_body_initial unit="deg"></alpha_body_initial>
					<alpha_body_final unit="deg"></alpha_body_final>
					<number_of_alpha_body></number_of_alpha_body>
					<alpha_wing_array_for_distributions unit="deg"></alpha_wing_array_for_distributions>
					<tau_elevator_function>
						<tau>0.025</tau>
						<delta_elevator unit="deg"></delta_elevator> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_elevator_function>
				</take_off_condition>					
				<climb_condition>
					<alpha_body_initial unit="deg"></alpha_body_initial>
					<alpha_body_final unit="deg"></alpha_body_final>
					<number_of_alpha_body></number_of_alpha_body>
					<alpha_wing_array_for_distributions unit="deg"></alpha_wing_array_for_distributions>
					<tau_elevator_function>
						<tau>0.025</tau>
						<delta_elevator unit="deg"></delta_elevator> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_elevator_function>
				</climb_condition>					
				<cruise_condition>
					<alpha_body_initial unit="deg"></alpha_body_initial>
					<alpha_body_final unit="deg"></alpha_body_final>
					<number_of_alpha_body></number_of_alpha_body>
					<alpha_wing_array_for_distributions unit="deg"></alpha_wing_array_for_distributions>
					<tau_elevator_function>
						<tau>0.025</tau>
						<delta_elevator unit="deg"></delta_elevator> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_elevator_function>
				</cruise_condition>	
				<landing_condition>
					<alpha_body_initial unit="deg"></alpha_body_initial>
					<alpha_body_final unit="deg"></alpha_body_final>
					<number_of_alpha_body></number_of_alpha_body>
					<alpha_wing_array_for_distributions unit="deg"></alpha_wing_array_for_distributions>
					<tau_elevator_function>
						<tau>0.025</tau>
						<delta_elevator unit="deg"></delta_elevator> <!-- if the tau array contains only 1 value this tag can be left empty -->
					</tau_elevator_function>
				</landing_condition>					
				<number_of_points_semispawise_wing></number_of_points_semispawise_wing>
				<number_of_points_semispawise_horizontal_tail></number_of_points_semispawise_horizontal_tail>
				<alpha_horizontal_tail_array_for_distributions unit="deg"></alpha_horizontal_tail_array_for_distributions>
				<delta_elevator_array unit="deg"></delta_elevator_array>
				<!-- optional values which are read only if the related attribute in "global_data" is "TRUE" --> 
				<horizontal_tail_dynamic_pressure_ratio></horizontal_tail_dynamic_pressure_ratio>
			</global_data>
			<wing_analyses>
				<aerodynamic_centre perform="TRUE" method="DEYOUNG_HARPER"/> <!-- referred to the mean aerodynamic chord --> 
				<lift>
					<cL_Alpha perform="TRUE" method="NASA_BLACKWELL"/>
					<cL_vs_alpha perform="TRUE" method="NASA_BLACKWELL"/>
					<alpha_zero_lift perform="TRUE" method="INTEGRAL_MEAN_TWIST"/>
				</lift>
				<high_lift>
				
				</high_lift>
				<drag>
				
				</drag>
				<pitching_moment>
				
				</pitching_moment>
			</wing_analyses>
			<horizontal_tail_analyses>
				<aerodynamic_centre perform="TRUE" method="DEYOUNG_HARPER"></aerodynamic_centre> <!-- referred to the mean aerodynamic chord --> 
				<dynamic_pressure_ratio perform="TRUE" method="SEMPIEMPIRICAL"></dynamic_pressure_ratio>
				<lift>
				
				</lift>
				<high_lift>
				
				</high_lift>
				<drag>
				
				</drag>
				<pitching_moment>
				
				</pitching_moment>
			</horizontal_tail_analyses>
			<vertical_tail_analyses>
				<lift>
				
				</lift>
				<high_lift>
				
				</high_lift>
				<drag>
				
				</drag>
				<pitching_moment>
				
				</pitching_moment>
			</vertical_tail_analyses>
			<fuselage_analyses>
			
			</fuselage_analyses>
			<nacelles_analyses>
			
			</nacelles_analyses>
			<aircraft_analyses>
				<downwash perform="TRUE" downwash_gradient="CONSTANT" method="SLINGERLAND"/>
				<lift_curve perform="TRUE"/>
				<drag_polar perform="TRUE"/>
				<moment_curve perform="TRUE"/>
				<trimmed_lift_curve perform="TRUE"/>
				<trimmed_drag_polar perform="TRUE"/>
				<trimmed_moment_curve perform="TRUE"/>
				<longitudinal_static_stability perform="TRUE"/>
				<lateral_static_stability perform="TRUE"/>
				<directional_static_stability perform="TRUE"/>
			</aircraft_analyses>
        </aerodynamics_and_stability>
    </analysis>
	<plot>
		<wing>
			<lift>
				<!-- choose which plot has to be done --> 
				<cL_vs_alpha perform="TRUE"/>
				<stall_path perform="TRUE"/> <!-- it uses NASA_BLACKWELL and is not linked to any analysis-->
				<cl_distribution perform="TRUE"/>
				<cl_additional_distribution perform="TRUE"/>
				<cl_basic_distribution perform="TRUE"/>
				<ccl_distribution perform="TRUE"/>
				<ccl_additional_distribution perform="TRUE"/>
				<ccl_basic_distribution perform="TRUE"/>
				<gamma_distribution perform="TRUE"/>
				<gamma_additional_distribution perform="TRUE"/>
				<gamma_basic_distribution perform="TRUE"/>
				<total_load_distribution perform="TRUE"/>
				<additional_load_distribution perform="TRUE"/>
				<basic_load_distribution perform="TRUE"/>
			</lift>
			<high_lift>
				<cL_vs_alpha_high_lift perform="TRUE"/>
			</high_lift>
			<drag>
			
			</drag>
			<pitching_moment>
				
			</pitching_moment>
		</wing>
		<horizontal_tail>
			<lift>
				<!-- choose which plot has to be done --> 
				<cL_vs_alpha perform="TRUE"/>
				<stall_path perform="TRUE"/> <!-- it uses NASA_BLACKWELL and is not linked to any analysis-->
				<cl_distribution perform="TRUE"/>
				<cl_additional_distribution perform="TRUE"/>
				<cl_basic_distribution perform="TRUE"/>
				<ccl_distribution perform="TRUE"/>
				<ccl_additional_distribution perform="TRUE"/>
				<ccl_basic_distribution perform="TRUE"/>
				<gamma_distribution perform="TRUE"/>
				<gamma_additional_distribution perform="TRUE"/>
				<gamma_basic_distribution perform="TRUE"/>
				<total_load_distribution perform="TRUE"/>
				<additional_load_distribution perform="TRUE"/>
				<basic_load_distribution perform="TRUE"/>
			</lift>
			<high_lift>
				<cL_vs_alpha_high_lift perform="TRUE"/>
			</high_lift>
			<drag>
			
			</drag>
			<pitching_moment>
				
			</pitching_moment>
		</horizontal_tail>
		<vertical_tail>
			<lift>
				<!-- choose which plot has to be done --> 
				<cL_vs_alpha perform="TRUE"/>
				<stall_path perform="TRUE"/> <!-- it uses NASA_BLACKWELL and is not linked to any analysis-->
				<cl_distribution perform="TRUE"/>
				<cl_additional_distribution perform="TRUE"/>
				<cl_basic_distribution perform="TRUE"/>
				<ccl_distribution perform="TRUE"/>
				<ccl_additional_distribution perform="TRUE"/>
				<ccl_basic_distribution perform="TRUE"/>
				<gamma_distribution perform="TRUE"/>
				<gamma_additional_distribution perform="TRUE"/>
				<gamma_basic_distribution perform="TRUE"/>
				<total_load_distribution perform="TRUE"/>
				<additional_load_distribution perform="TRUE"/>
				<basic_load_distribution perform="TRUE"/>
			</lift>
			<high_lift>
				<cL_vs_alpha_high_lift perform="TRUE"/>
			</high_lift>
			<drag>
			
			</drag>
			<pitching_moment>
				
			</pitching_moment>
		</vertical_tail>
		<fuselage>
		
		</fuselage>
		<nacelles>
		
		</nacelles>
		<aircraft>
		
		</aircraft>
	</plot>
</jpad_config>
