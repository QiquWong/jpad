<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <analysis>
        <performance
			id="ATR72 - PERFORMANCE"
			weights_from_xls_file="false" 
			file_weights="Weights.xlsx"
			aerodynamics_from_xls_file="false" 
			file_aerodynamics="Aerodynamics.xlsx"
			>
			<weights>
				<maximum_take_off_mass unit="kg">22500.0</maximum_take_off_mass>
				<maximum_landing_mass unit="kg">20250.0</maximum_landing_mass>
				<maximum_zero_fuel_mass unit="kg">20300.0</maximum_zero_fuel_mass>
				<operating_empty_mass unit="kg">12950.0</operating_empty_mass>
				<maximum_fuel_mass unit="kg">5000.0</maximum_fuel_mass>
			</weights>
			<aerodynamics parabolicDragPolar="TRUE"> <!-- if TRUE uses a parabolic drag polar with wave drag correction; otherwise it reads the polar_curves tag -->
				<current_lifting_coefficient></current_lifting_coefficient>
				<current_drag_coefficient></current_drag_coefficient>
				<cD0>0.0281</cD0>
				<oswald>0.83</oswald> 
				<oswald_take_off>0.81</oswald_take_off>
				<oswald_landing>0.79</oswald_landing>
				<cLmax_clean_configuration>1.6</cLmax_clean_configuration>
				<cL_alpha_clean_configuration unit="1/deg">0.09</cL_alpha_clean_configuration>
				<cL_alpha_take_off unit="1/deg">0.0905</cL_alpha_take_off>
				<cL_alpha_landing unit="1/deg">0.0925</cL_alpha_landing>
				<delta_CD0_take_off>0.0167</delta_CD0_take_off>
				<delta_CD0_landing>0.0673</delta_CD0_landing>
				<delta_CD0_landing_gears>0.014</delta_CD0_landing_gears>
				<delta_CD0_spoilers>0.011</delta_CD0_spoilers>
				<cLmax_take_off_configuration>2.05</cLmax_take_off_configuration>
				<cL0_take_off_configuration>0.801</cL0_take_off_configuration>
				<cLmax_landing_configuration>2.63</cLmax_landing_configuration>
				<cL0_landing_configuration>1.2577</cL0_landing_configuration>
				<polar_curves>
					<cruise>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</cruise>
					<take_off>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</take_off>
					<landing>
						<polar_curve_CL></polar_curve_CL>
						<polar_curve_CD></polar_curve_CD>
					</landing>
				</polar_curves>
			</aerodynamics>
			<!-- the following data can be assigned between tags, or not. If not, default values are used -->
			<takeoff_landing>
				<dt_rotation unit="s"></dt_rotation> <!-- the duration of the rotation phase (default = 3s) -->
				<dt_hold unit="s"></dt_hold> <!-- the duration of the constant CL phase (default = 0.5s) -->
				<alpha_ground unit="deg"></alpha_ground> <!-- angle of attack in BRF along the runway (dafault = 0Â°)-->
				<wind_speed_along_runway unit="m/s"></wind_speed_along_runway> <!-- positive if behind the aircraft (default = 0m/s)-->
				<obstacle_take_off unit="ft"></obstacle_take_off> <!-- default = 35 ft -->
				<wheels_friction_coefficient></wheels_friction_coefficient> <!-- default = 0.03 -->
				<wheels_friction_coefficient_with_brakes>0.5</wheels_friction_coefficient_with_brakes> <!-- default = 0.4 -->
				<k_rotation></k_rotation> <!-- V_rotation/V_stall in take-off (default = 1.05) -->
				<k_lift_off>1.13</k_lift_off> <!-- V_lift-off/V_stall in take-off (default = 1.1) -->
				<k_cLmax>0.85</k_cLmax> <!-- percentage of the CLmax,TO not to be surpassed during the rotation  (default = 0.9)-->
				<k_drag_due_to_engine_failure></k_drag_due_to_engine_failure> <!-- percentage of the drag coefficent increase in OEI condition (default = 1.1) -->
				<k_alpha_dot>0.05</k_alpha_dot> <!-- indicates how fast the rotation maneuver is - faster with increasing k_alpha_dot - (default 0.04) -->
				<alpha_reduction_rate unit="deg/s">-3</alpha_reduction_rate> <!-- negative angular velocity after reaching max allowed CL in rotation (default = -4 deg/s)-->
				<obstacle_landing unit="ft"></obstacle_landing> <!-- default = 50ft -->
				<theta_approach unit="deg"></theta_approach> <!-- default = 3deg -->
				<k_approach></k_approach> <!-- V_approach/V_stall in landing (default = 1.3) -->
				<k_flare></k_flare> <!-- V_flare/V_stall in landing (default = 1.23) -->
				<k_touch_down></k_touch_down> <!-- V_touch_down/V_stall in landing (default = 1.15) -->
				<free_roll_duration unit="s"></free_roll_duration> <!-- default = 2s -->
			</takeoff_landing>
			<!-- the following data can be assigned or not. If not default values are used -->
			<other_data>
				<throttle_setting></throttle_setting> <!-- to be assigned for the wanted performance (default = 0.0) -->
				<cLmax_inverted></cLmax_inverted> <!-- to be assigned for the maneuvering flight and gust envelope (default = -1.0) --> 
			</other_data>
        </performance>
    </analysis>
	<plot>
		<takeoff>
			<simulations perform="TRUE"/>
			<balanced_field_length perform="TRUE"/>
		</takeoff>
		<climb>
			<!-- ADD CHARTS -->
		</climb>
		<cruise>
			<thrust_drag perform="TRUE"/>
			<efficiency perform="TRUE"/>
			<sfc perform="TRUE"/>
			<cruise_grid perform="TRUE"/>
			<flight_envelope perform="TRUE"/>
			<!-- ADD CHARTS -->
		</cruise>
		<landing>
			<simulations perform="TRUE"/>
		</landing>
		<payload_range perform="TRUE"/>
		<maneuvering_flight_and_gust_envelope perform="TRUE"/>
	</plot>
</jpad_config>
