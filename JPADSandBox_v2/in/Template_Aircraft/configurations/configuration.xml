<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <configuration id="JPAD Test Cabin Configuration DAF - 2016">
		<global_data>
			<actual_passengers_number>50</actual_passengers_number>
			<maximum_passengers_number>90</maximum_passengers_number>
			<flight_crew_number>2</flight_crew_number>
			<classes_number>3</classes_number>
			<classes_type>ECONOMY;BUSINESS;FIRST</classes_type>
			<aisles_number>1</aisles_number>
			<x_coordinates_first_row unit="m">5.5</x_coordinates_first_row>
			<seat_block_position>RIGHT</seat_block_position> <!--LEFT means the block is in the y axis negative side. 
																 RIGHT means the block is in the y axis positive side.-->
			<missing_seat_row>
				<!--
				The size of missing seat list has to be equal to the class size.
				If missing seat is empty, the seats number for that class is 
				given by rows*columns; otherwise it's given by 
				(rows*columns)-missing_seats_row.
				If the user wants to add other arrays it has to add another value tag.
				Each value tag contains an array of Integer values and must be assigned using [ , , ].
				-->
				<value>[1,2,3]</value>
				<value>[1,2,3]</value>
				<value>[1,2,3]</value>
			</missing_seat_row> 
		</global_data>
		<detailed_data>
			<number_of_breaks_economy_class>0</number_of_breaks_economy_class>
			<number_of_breaks_business_class>0</number_of_breaks_business_class>
			<number_of_breaks_first_class>0</number_of_breaks_first_class>
			<number_of_rows_economy_class>15</number_of_rows_economy_class>
			<number_of_rows_business_class>7</number_of_rows_business_class>
			<number_of_rows_first_class>3</number_of_rows_first_class>
			<number_of_columns_economy_class>3;3</number_of_columns_economy_class>
			<number_of_columns_business_class>2;2</number_of_columns_business_class>
			<number_of_columns_first_class>2;2</number_of_columns_first_class>
			<pitch_economy_class unit="m">0.85</pitch_economy_class>
			<pitch_business_class unit="m">0.95</pitch_business_class>
			<pitch_first_class unit="m">1.05</pitch_first_class>
			<width_economy_class unit="m">0.5</width_economy_class>
			<width_business_class unit="m">0.6</width_business_class>
			<width_first_class unit="m">0.7</width_first_class>
			<distance_from_wall_economy_class unit="m">0.09</distance_from_wall_economy_class>
			<distance_from_wall_business_class unit="m">0.09</distance_from_wall_business_class>
			<distance_from_wall_first_class unit="m">0.09</distance_from_wall_first_class>
		</detailed_data>
		<reference_masses>
			<mass_furnishings_and_equipment unit="kg">2500</mass_furnishings_and_equipment>
		</reference_masses>
    </configuration>
</jpad_config>