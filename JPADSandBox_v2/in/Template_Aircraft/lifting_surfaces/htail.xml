<jpad_config>
    <horizontal_tail id="JPAD Test Horizontal Tail DAF - 2016" mirrored="TRUE">
        <panels>
            <panel id="Horizontal tail panel">
                <span unit="m">6.25</span>
                <dihedral unit="deg">0.0</dihedral>
                <sweep_leading_edge unit="deg">25</sweep_leading_edge>
                <inner_section>
                    <chord unit="m">3.0</chord>
                    <airfoil file="naca0012.xml"/>
                    <geometric_twist unit="deg">0.0</geometric_twist>
                </inner_section>
                <outer_section>
                    <chord unit="m">1.015</chord>
                    <airfoil file="naca0012.xml"/>
                    <geometric_twist unit="deg">0.0</geometric_twist>
                </outer_section>
            </panel>
        </panels>
        <symmetric_flaps>
            <symmetric_flap id="Elevator" type="PLAIN">
                <inner_station_spanwise_position type="PERCENT_SEMISPAN" ref_to="FULL_SEMISPAN">0.1</inner_station_spanwise_position>
                <outer_station_spanwise_position type="PERCENT_SEMISPAN" ref_to="FULL_SEMISPAN">0.9</outer_station_spanwise_position>
                <inner_chord_ratio>0.30</inner_chord_ratio>
				<outer_chord_ratio>0.30</outer_chord_ratio>
                <min_deflection unit="deg">-25.0</min_deflection>
				<max_deflection unit="deg">5.0</max_deflection>
            </symmetric_flap>
        </symmetric_flaps>
    </horizontal_tail>
</jpad_config>