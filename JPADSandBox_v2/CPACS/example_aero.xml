<?xml version="1.0"?>

<aerodynamics>

  <function name="aero/function/lift_coeff_basic_M0" >
    <description>CL = f(alpha, beta, Re) at M0=0.3</description>
    <table>
      <independentVar lookup="row">aero/alpha-deg</independentVar>
      <independentVar lookup="column">aero/beta-deg</independentVar>
      <independentVar lookup="table">aero/Re</independentVar>
      <tableData breakpoint="3000000"> <!-- Reynolds -->
              0.0   5.0
         0.0  0.02  0.001
         5.0  0.12  0.03
        10.0  0.30  0.08
      </tableData>
      <tableData breakpoint="6000000"> <!-- Reynolds -->
              0.0   5.0
         0.0  0.05  0.013
         5.0  0.31  0.23
        10.0  0.67  0.28
      </tableData>
    </table>
  </function>

  <function name="aero/function/lift_coeff_basic_M1" >
    <description>CL = f(alpha, beta, Re) at M1=0.6</description>
    <table>
      <independentVar lookup="row">aero/alpha-deg</independentVar>
      <independentVar lookup="column">aero/beta-deg</independentVar>
      <independentVar lookup="table">aero/Re</independentVar>
      <tableData breakpoint="3000000"> <!-- Reynolds -->
              0.0   5.0
         0.0  0.02  0.001
         5.0  0.12  0.03
        10.0  0.30  0.08
      </tableData>
      <tableData breakpoint="6000000"> <!-- Reynolds -->
              0.0   5.0
         0.0  0.05  0.013
         5.0  0.31  0.23
        10.0  0.67  0.28
      </tableData>
    </table>
  </function>

  <axis name="LIFT">
    <function name="aero/forces/lift_basic_Mach">
      <description>
        Lift due to alpha, beta, Reynolds, Mach
      </description>
      <product>
        <property>aero/qbar-psf</property>
        <property>metrics/Sw-sqft</property>
        <table>
          <independentVar lookup="row">velocities/mach</independentVar>
          <!-- ?????? -->
          <tableData>
            0.3 <property>aero/function/lift_coeff_basic_M0</property>
            0.6 <property>aero/function/lift_coeff_basic_M1</property>
          </tableData>
      <product>
      <function>
  </axis>
  
  <!-- 
    https://sourceforge.net/p/jsbsim/mailman/message/31037613/
  -->
  <function name="aero/forces/lift_basic_Mach">
      <description>
        Lift due to alpha, beta, Reynolds, Mach
      </description>
      <product>
        <property>aero/qbar-psf</property>
        <property>metrics/Sw-sqft</property>
        <interpolate1d>
          <p> velocities/mach </p>
          <v> 0.30 </v> <function>aero/function/lift_coeff_basic_M0</function>
          <v> 0.60 </v> <function>aero/function/lift_coeff_basic_M1</function>
        </interpolate1d>
      </product>
  </function>  
  
  
</aerodynamics>
