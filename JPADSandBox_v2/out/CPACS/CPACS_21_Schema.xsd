						<ddue:para>longFloorBeamPosition type, containing individual position definition
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element maxOccurs="1" minOccurs="1" name="structuralElementUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation> UID of structural element</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="1" name="crossBeamUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation> UID of crossbeam to which the long. beam is attached
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="1" name="positionY" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation> y position of long. beam</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="alignment" type="alignmentStructMemberType">
					</xsd:element>
					<xsd:element default="0" maxOccurs="1" minOccurs="0" name="continuity">
						<xsd:annotation>
							<xsd:documentation> Continuity definition for profile extrusion: 0= C0 (allows sharp edges,
                                default), 2= C2 (defines curvature continuity)
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:enumeration value="0" />
									<xsd:enumeration value="2" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element default="0" maxOccurs="1" minOccurs="0" name="interpolation">
						<xsd:annotation>
							<xsd:documentation> Definition of interpolation between different profiles: 0= no
                                interpolation 1= interpolation of strcutural profile
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:enumeration value="0" />
									<xsd:enumeration value="1" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="optional" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
