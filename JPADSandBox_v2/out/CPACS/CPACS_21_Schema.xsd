						<ddue:para> EnginePerformanceMap type, containing a perfomance map with engine data
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType" />
					<xsd:element minOccurs="0" name="description" type="stringBaseType" />
					<xsd:element minOccurs="0" name="flightLevel" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Flight Level</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="machNumber" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mach number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="thrust" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Absolute thrust [N]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="mDotFuel" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Fuel mass flow</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="v8" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Speed at core engine nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="t8" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Total temperature at core engine nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="mDot8" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mass flow through core engine nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="v18" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Speed at bypass nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="t18" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Total temperature at bypass nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="mDot18" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mass flow through bypass nozzle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="n1" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Percent of n1Max, shaft 1</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="n2" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Percent of n2Max, shaft 2</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="piFan" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Fan pressure ratio</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="etaPFan" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Fan effiiency</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="tET" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Turbine entry total temperature</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="eiCO" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Emission index Carbon Monoxide</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="eiNOx" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Emission index Nitrogen Oxide</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="eiSoot" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Emission index Soot</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="eiHC" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Emission index unburned hydrocarbone</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="rho8" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>air density at core outlet 8</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="rho18" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>air density at bypass outlet 18</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="nozzA8" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>area at core outlet</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element maxOccurs="1" minOccurs="0" name="nozzA18" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>area at bypass outlet </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
