	<xsd:complexType name="modGenOptionsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>modGenOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>ModGenOptions type, containing ModGen options</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="divergenceMachCases" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mach numbers for divergence analysis cases. Up to 6 Ma Cases can be
                                defined to analyze the divergence behavior for the according points on the flight
                                envelope
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="optimizationMethod">
						<xsd:annotation>
							<xsd:documentation>Method for internal optimization: Modified Method of Feasible Directions
                                [1] (default); Sequential Linear Programming [2]; Sequential Quadratic Programming [3]
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:minInclusive value="1" />
									<xsd:maxInclusive value="3" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="landingGear" type="modGenOptionalComponentsType" />
					<xsd:element minOccurs="0" name="engines" type="modGenOptionalComponentsType">
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
