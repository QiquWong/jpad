					<xsd:element maxOccurs="1" minOccurs="0" name="hingeTranslation" type="pointXYZType">
						<xsd:annotation>
							<xsd:documentation>Translation along the x-, y- and z-Coordinate of the rotated hinge coordinate system.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
