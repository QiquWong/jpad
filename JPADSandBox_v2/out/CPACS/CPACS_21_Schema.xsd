					</xsd:element>

					<xsd:element name="upperB" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>B Coefficients for the Bernstein polynominal on the upper side</xsd:documentation>
						</xsd:annotation>                
					</xsd:element>
					<xsd:element name="lowerN1" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>N1 for the class function for the lower side of the profile</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lowerN2" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>N2 for the class function for the lower side of the profile</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lowerB" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>B Coefficients for the Bernstein polynominal on the lower side</xsd:documentation>
						</xsd:annotation>                
					</xsd:element>

				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
