	<xsd:complexType name="deckType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>deckType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Deck type, containing the data of a deck</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the deck</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the deck</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="parentUID" type="stringUIDBaseType">
					</xsd:element>
					<xsd:element name="x0" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>The starting point of the deck/cabin. In a conventional aircraft like the A320, it would be the rear wall of the cockpit. The coordinate is relative to the parent object defined by &#8220;parentUID&#8221;, which should be  the fuselage. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="z0" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>The starting point of the deck/cabin. In a conventional aircraft like the A320, it would be the rear wall of the cockpit. The coordinate is relative to the parent object defined by &#8220;parentUID&#8221;, which should be  the fuselage. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="type">
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="passenger" />
									<xsd:enumeration value="VIP" />
									<xsd:enumeration value="cargo" />
									<xsd:enumeration value="lifeStock" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="cabGeometry" type="cabGeometryType">
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="aisles" type="cabinAislesType">
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="spaces" type="cabinSpacesType">
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="doors" type="cabinDoorsType">
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="seatElements" type="cabinSeatElementsType">
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="floorElements" type="cabinFloorElementsType">
