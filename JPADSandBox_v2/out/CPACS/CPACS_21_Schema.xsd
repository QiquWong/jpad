	<xsd:complexType name="gearComponentsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Detailed description of all components of the main landing gear.
						</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:content>
							<ddue:para>Detailed description of the following components of the landing gear: mainStrut,
                                piston, dragStrut, pintleStrut, sideStrut, mainActuator, bogie, axle and wheels.
							</ddue:para>
							<ddue:para>The definition of the components include geometrical as well as material
                                properties.
							</ddue:para>
							<ddue:para>The definition of the different components can be found in the pictures below for
                                the main and the nose landing gear:
							</ddue:para>
							<ddue:mediaLink>
								<ddue:image ns3:href="mlg" xmlns:ns3="http://www.w3.org/1999/xlink" />
							</ddue:mediaLink>
							<ddue:mediaLink>
								<ddue:image ns3:href="nlg" xmlns:ns3="http://www.w3.org/1999/xlink" />
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="mainStrut" type="strutType" />
					<xsd:element name="piston" type="pistonType" />
					<xsd:element name="dragStrut" type="strutWithActuatorType" />
					<xsd:element name="pintleStrut" type="strutType" />
					<xsd:element name="sideStruts" type="sideStrutsType" />
					<xsd:element name="mainActuator" type="mainActuatorType" />
					<xsd:element minOccurs="0" name="bogie" type="bogieType" />
					<xsd:element name="axles" type="axlesType" />
					<xsd:element name="wheels" type="wheelsType" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
