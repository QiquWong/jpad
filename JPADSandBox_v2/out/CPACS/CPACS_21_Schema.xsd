	<xsd:complexType name="modGenOptionalComponentsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>modGenOptionalComponentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>ModGenOptional components type, containing optional components to be consideref in
                            ModGen calculations
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="calculation" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Switch to include component into calculation
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="attachmentPoint">
						<xsd:annotation>
							<xsd:documentation>Attachment point to the structure</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="ALLCP" />
									<xsd:enumeration value="FRONT" />
									<xsd:enumeration value="REAR" />
									<xsd:enumeration value="LOWER" />
									<xsd:enumeration value="UPPER" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="attachmentRibs" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of ribs used for attaching the component
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
