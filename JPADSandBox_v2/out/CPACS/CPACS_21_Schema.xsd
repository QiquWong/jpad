	</xsd:complexType>
	<xsd:complexType name="spoilerType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Spoilers of the wing.</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>A spoiler is defined via its outerShape relative to the componentSegment. The
                            WingCutOut defines the area of the upper skin that is removed by the spoiler. Structure is
                            similar to the wing structure. The mechanical links between the spoiler and the parrent are
                            defined in tracks. The deflection path is described in path. Additional actuators, that are
                            not included into a track, can be defined in actuators.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the spoiler.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the spoiler.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the parent of the spoiler. The parent is the componentSegment,
                                where the spoiler is attached.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outerShape" type="controlSurfaceOuterShapeSpoilerType" />
					<xsd:element minOccurs="0" name="wingCutOut" type="controlSurfaceWingCutOutType" />
					<xsd:element minOccurs="0" name="structure" type="wingComponentSegmentStructureType" />
					<xsd:element name="path" type="controlSurfacePathType" />
					<xsd:element minOccurs="0" name="tracks" type="controlSurfaceTracksType" />
					<xsd:element minOccurs="0" name="actuators" type="controlSurfaceActuatorsType">
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
