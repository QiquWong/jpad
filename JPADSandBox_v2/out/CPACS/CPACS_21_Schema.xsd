					</xsd:element>
					<xsd:element minOccurs="0" name="z" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Z-Component</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="optional" />
				<xsd:attribute default="absLocal" name="refType" use="optional">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="absGlobal" />
							<xsd:enumeration value="absLocal" />
							<xsd:enumeration value="relative" />
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
