					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>trafumoTool type, containing transport aircraft fuselage
                            modelling tool </ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="1" name="analysisType">
						<xsd:annotation>
							<xsd:documentation>Definition of the analysis type used in TRAFUMO,
                                restricted to values 'modelling', 'analysis' and 'sizing'
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="modelling" />
									<xsd:enumeration value="analysis" />
									<xsd:enumeration value="sizing" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType" />
					<xsd:element name="aircraftFuselageUID" type="stringUIDBaseType" />
					<xsd:element minOccurs="0" name="aircraftWingUID" type="stringUIDBaseType" />
					<xsd:element minOccurs="0" name="aircraftHtpUID" type="stringUIDBaseType" />
					<xsd:element minOccurs="0" name="aircraftVtpUID" type="stringUIDBaseType" />
					<xsd:element minOccurs="0" name="createMetaData" type="booleanBaseType" />                    
					<xsd:element minOccurs="0" name="plotFormat">
						<xsd:annotation>
							<xsd:documentation>Definition of the graphics plot format to be
                                generated in TRAFUMO (png, jpg or bmp, default: *.png)
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="png" />
									<xsd:enumeration value="PNG" />
									<xsd:enumeration value="jpg" />
