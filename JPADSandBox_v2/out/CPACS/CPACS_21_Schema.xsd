									<xsd:enumeration value="full_Random" />
									<xsd:enumeration value="3/4_Sorted" />
									<xsd:enumeration value="3/4_Random" />
									<xsd:enumeration value="1/2_Sorted" />
									<xsd:enumeration value="1/2_Random" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
