	<xsd:complexType name="routeType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Describes a specific route</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Describes a specific route, e.g. FRA-EWR, which is operated by a speicifc air vehicle
                            (modelUID) on a specific mission profile (missionUID). The route is further described by the
                            UTC starting time.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the route</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the route</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="modelUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>The aircraft operated on this route</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="missionUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>The mission specified for this route</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="utcStartingTime" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation> Starting time of the flight expressed as Greenwich (Zulu) time in UTC
                                format: JJJJ:MM:DDThh:mm:ss, e.g. 2011:06:16T12:31:51
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="frequency" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation> The frequency at which this flight is operated in flights per year
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="paxLoadFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation> The specific pax load factor for this route, ranges from 0...1
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="cargoLoadFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation> The specific cargo load factor for this route, ranges from 0...1
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="optional" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
