	<xsd:complexType name="loadReferenceType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Reference values for local aerodynamic coefficients.</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains the reference values used for normalizing local
                                        aerodynamic coefficients. The reference point is defined in absolut CPACS-coordinates.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>
                                    1. &lt;point&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Reference point, specified in absolut CPACS-coordinates The reference
                                        point is used as origin for calculating the local moment coefficients. See the documentation of the global CPACS-Element for further information. 
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>
                                    1. &lt;area&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Local reference area.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>
                                    2. &lt;length&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Local reference length, used for normalizing all the moment coefficients. The length is always given in the middle of the wing segment.  
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="area" type="doubleBaseType" />
					<xsd:element name="length" type="doubleBaseType" />
					<xsd:element name="point" type="pointXYZType" />
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="longFloorBeamPositionType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
