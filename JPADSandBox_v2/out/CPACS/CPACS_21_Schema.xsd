					<xsd:element minOccurs="0" name="touchDown" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Safety margin for landing gear x position to avoid tail dropping down
                                during touchDown and ground maneuvering
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="turnOver" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Safety margin for landing gear y position to avoid wing tip dropping down
                                during ground maneuvering
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="rollAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Safety margin for landing gear y position regarding wingtip or engine
                                nacelle clearance at a certein roll angle
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="landingGearSettingType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>landingGearSettingType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para />
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="landingGearUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a landing gear</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="extended" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Extended (1=true, 2=false)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="relDeflection" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative deflection of the landing gear
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="landingGearType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Landing gears of the aircraft model.</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LandingGear type, containing the definition of nose and main landing gears.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="noseGears" type="noseGearsType" />
					<xsd:element minOccurs="0" name="mainGears" type="mainGearsType" />
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lateralFlightPlanType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lateralFlightPlanType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lateralFlightPlanType, describing the lateral flight plan of the mission. Values are
                            semicolon seperated as usual in CPACS. If waypoint codes are not available put entry items
                            into the waypoint string and enter only lat,long coordinates. Speed and altitude constraints
                            can be added as optional parameters.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="waypoints" type="stringVectorBaseType" />
					<xsd:element name="latitude" type="stringVectorBaseType" />
					<xsd:element name="longitude" type="stringVectorBaseType" />
					<xsd:element minOccurs="0" name="minSpeedCAS" type="stringVectorBaseType" />
					<xsd:element minOccurs="0" name="maxSpeedCAS" type="stringVectorBaseType" />
					<xsd:element minOccurs="0" name="minAltitude" type="stringVectorBaseType" />
					<xsd:element minOccurs="0" name="maxAltitude" type="stringVectorBaseType" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="leadingEdgeDeviceType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Trailing edge device of the wing.</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>A leadingEdgeDevice (LED) is defined via its outerShape relative to the
                            componentSegment. The WingCutOut defines the area of the skin that is removed by the LED.
                            Structure is similar to the wing structure. The mechanical links between the LED and the
                            parrent are defined in tracks. The deflection path is described in path. Additional
                            actuators, that are not included into a track, can be defined in actuators.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the leading edge device.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the leading edge device.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the parent of the LED. The parent is the componentSegment, where
                                it is attached to.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outerShape" type="controlSurfaceOuterShapeLeadingEdgeType" />
					<xsd:element minOccurs="0" name="wingCutOut" type="controlSurfaceWingCutOutType" />
					<xsd:element minOccurs="0" name="structure" type="wingComponentSegmentStructureType" />
					<xsd:element name="path" type="controlSurfacePathType" />
					<xsd:element minOccurs="0" name="tracks" type="controlSurfaceTracksType" />
					<xsd:element minOccurs="0" name="actuators" type="controlSurfaceActuatorsType">
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="leadingEdgeDevicesType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Definition of the wings leading edge devices.</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Definition of the wings leading edge devices.</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="leadingEdgeDevice" type="leadingEdgeDeviceType" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="leadingEdgeHollowType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Optional definition of the airfoil inner shape of leading edge devices (LED).
						</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:content>
							<ddue:para>All parameters are optional. For the definition of the parameters, please refer
                                to the picture below. Parameters from the outer border default to the parameters of the
                                inner border.
							</ddue:para>
							<ddue:mediaLink>
								<ddue:image ns3:href="led" xmlns:ns3="http://www.w3.org/1999/xlink" />
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="relHeightTE" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative hight of the most forward point of the LED's rear part, based on
                                the airfoil height of the parent at this position. Optional.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="xsiTE" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative chordwise position of the most forward point of the LED's rear
                                part, based on the chord of the parent at this position. Optional.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="leadingEdgeShapeType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>Optional definition of the leading edge shape of trailing edge devices (TED).
						</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:content>
							<ddue:para>All parameters are optional. For the definition of the parameters, please refer
                                to the picture below. Parameters from the outer border default to the parameters of the
                                inner border.
							</ddue:para>
							<ddue:mediaLink>
								<ddue:image ns3:href="outerShape2" xmlns:ns3="http://www.w3.org/1999/xlink" />
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="relHeightLE" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative hight of the leading edge of the TED, based on the airfoil
                                height of the parent at this position. Optional.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="xsiUpperSkin" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative chordwise upper skin position, of the border, where the airfoil
                                of the TED is equivalent of the airfoil from the parent. Measured from the rear to the
                                front (0 = TED trailing edge; 1 = TED leading edge). Values form the outer border
                                default to the value of the inner border. Optional.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="xsiLowerSkin" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative chordwise lower skin position, of the border, where the airfoil
                                of the TED is equivalent of the airfoil from the parent. Measured from the rear to the
                                front (0 = TED trailing edge; 1 = TED leading edge). Values form the outer border
                                default to the value of the inner border. Optional.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignGearType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignGearType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignGear type, containing toolspecific data for the design of a gear
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="gearUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the gear</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="oleoGasLength" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Oleo gas length of the LG shock strut in meter. Common value is +/- 10%
                                different to the stroke
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="wheels">
						<xsd:annotation>
							<xsd:documentation>Number of wheels</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="2" />
									<xsd:enumeration value="4" />
									<xsd:enumeration value="6" />
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="intDampCoeff" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>LG shock strut damping coefficient in N/(m/s)^2
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignGeneralParametersType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignGeneralParametersType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignGeneralParameters type, containing general parameters for LGDesign
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="tireEfficiency" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Tire efficiency coefficient for the kinetic energy absortion. Typcal
                                value is 0.47
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="shockEfficiency" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Shock absorber efficiency coefficient for the kinetic energy absortion.
                                Typcal value is 0.8
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxTurnOverAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum allowable turn over angle in degrees
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxLandRollAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum allowable roll angle during landing and take off
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxTouchDownOffsetAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum allowable LG offset angle to the C.G. position
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="allowLGxPosDiff" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Minimum allowble x distance in meters between each MLG. No use for TIVA
                                and VAMP!!
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="allowLGyPosDiff" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Minimum allowble y distance in meters between each MLG. No use for TIVA
                                and VAMP!!
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxLandingAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum aircraft landing angle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxTakeOffPitchingAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum pitching angle at takeoff</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignGeometryPointsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignGeometryPointsType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignGeometryPoints type, containing specific points of the geometry for LGDesign.
                            Coordinates X and Y are given as absolute values in the global coordinate system, z is given
                            as absolute value, but as vertical offset to CoG.
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="tailTip" type="pointType">
						<xsd:annotation>
							<xsd:documentation>Trailing point of the geometry (highest x-coordinate)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="lowestPoint" type="pointType">
						<xsd:annotation>
							<xsd:documentation>Lowest point of the geometry (lowest z-coordinate)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignMainGearsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignMainGearsType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignMainGears type, containing toolspecific data for the main gear design
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="stroke" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>MLG stroke (Displacement the shock strut move during landing). Common for
                                tranpsort aircraft is 0.4-0.45
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="staticPressureShock" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>MLG static shock strut pressure in Psi
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" name="mainGear" type="lgDesignGearType">
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignNoseGearsType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignNoseGearsType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignNoseGears type, containing toolspecific data for the main gear design
						</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="stroke" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>NLG stroke (Displacement the shock strut move during landing). Common for
                                tranpsort aircraft is 0.4-0.45
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="staticPressureShock" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>NLG static shock strut pressure in Psi
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" name="noseGear" type="lgDesignGearType">
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="lgDesignToolType">


		<!-- ****************************************************
 -->
		<xsd:annotation>
			<xsd:appinfo>
				<schemaDoc xmlns="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3" xmlns:ddue="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xsd="http://schemas.xsddoc.codeplex.com/schemaDoc/2009/3">
					<ddue:summary xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>lgDesignToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks xmlns:xsd="http://ddue.schemas.microsoft.com/authoring/2003/5">
						<ddue:para>LgDesignTool type, containing LGDesign tool data</ddue:para>
					</ddue:remarks>
				</schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to aircraft model</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="wingUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to the wing which shall be calculated
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="rangesCoG" type="rangesCoGType" />
					<xsd:element name="geometryPoints" type="lgDesignGeometryPointsType" />
					<xsd:element name="generalParameters" type="lgDesignGeneralParametersType" />
					<xsd:element name="mainGears" type="lgDesignMainGearsType" />
					<xsd:element name="noseGears" type="lgDesignNoseGearsType" />
				</xsd:all>
			</xsd:extension>
