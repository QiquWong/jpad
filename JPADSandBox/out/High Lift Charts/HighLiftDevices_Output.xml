         <delta_CD0_total/>
         <delta_CM_c4_of_each_flap/>
         <delta_CM_c4_total/>
      </high_lift_devices_effects>
      <global_high_lift_devices_effects>
         <alpha_max_high_lift/>
         <alpha_star_high_lift/>
         <cLmax_high_lift/>
         <cL_star_high_lift/>
         <cL_alpha_high_lift/>
      </global_high_lift_devices_effects>
   </OUTPUT>
