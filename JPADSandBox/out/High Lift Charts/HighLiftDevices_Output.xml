            <slat_deflection/>
            <slat_chord_ratio/>
            <slat_extension_ratio/>
            <slat_non_dimensional_inner_station/>
            <slat_non_dimensional_outer_station/>
         </slat_1>
         <slat_2>
            <slat_deflection/>
            <slat_chord_ratio/>
            <slat_extension_ratio/>
            <slat_non_dimensional_inner_station/>
            <slat_non_dimensional_outer_station/>
         </slat_2>
         <slat_3>
            <slat_deflection/>
            <slat_chord_ratio/>
            <slat_extension_ratio/>
            <slat_non_dimensional_inner_station/>
            <slat_non_dimensional_outer_station/>
         </slat_3>
      </slats>
