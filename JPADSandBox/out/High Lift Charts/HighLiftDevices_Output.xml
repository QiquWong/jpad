<?xml version="1.0" encoding="UTF-8"?>
<High_Lift_Executable>
   <INPUT>
      <flight_condition>
         <alpha_current/>
      </flight_condition>
      <wing>
         <geometry>
            <aspect_ratio/>
            <span/>
            <surface/>
            <root_chord_equivalent_wing/>
            <sweep_quarter_chord_equivalent_wing/>
            <taper_ratio_equivalent_wing/>
         </geometry>
         <clean_configuration_parameters>
            <alpha_stall_clean/>
            <alpha_star_clean/>
            <cL0_clean/>
            <cLalpha_clean/>
            <cLmax_clean/>
            <cLStar_clean/>
         </clean_configuration_parameters>
         <mean_airfoil>
            <cl_alpha_mean_airfoil/>
            <cl0_mean_airfoil/>
            <leading_edge_radius_mean_airfoil/>
            <mean_airfoil_chord/>
            <max_thickness_mean_airfoil/>
            <airfoil_family/>
         </mean_airfoil>
      </wing>
      <flaps>
         <flap_1>
            <flap_type/>
            <flap_chord_ratio/>
            <flap_deflection/>
            <flap_non_dimensional_inner_station/>
            <flap_non_dimensional_outer_station/>
