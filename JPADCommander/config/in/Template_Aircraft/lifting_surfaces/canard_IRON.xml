<jpad_config>
    <canard id="IRON Canard" mirrored="TRUE">
         <global_data>
			<composite_correction_factor>0.83</composite_correction_factor>
			<roughness unit="m">0.405e-5</roughness>
		</global_data>
		<panels>
            <panel id="Canard panel">
                <span unit="m">4.0</span>
                <dihedral unit="deg">0.0</dihedral>
                <sweep_leading_edge unit="deg">10.0</sweep_leading_edge>
                <inner_section>
                    <chord unit="m">1.85</chord>
                    <airfoil file="CANARD_IRON.xml"/>
                    <geometric_twist unit="deg">0.0</geometric_twist>
                </inner_section>
                <outer_section>
                    <chord unit="m">0.784</chord>
                    <airfoil file="CANARD_IRON.xml"/>
                    <geometric_twist unit="deg">0.0</geometric_twist>
                </outer_section>
            </panel>
        </panels>
        <symmetric_flaps>
            <symmetric_flap id="Control Surface" type="PLAIN">
                <inner_station_spanwise_position type="PERCENT_SEMISPAN" ref_to="FULL_SEMISPAN">0.1</inner_station_spanwise_position>
                <outer_station_spanwise_position type="PERCENT_SEMISPAN" ref_to="FULL_SEMISPAN">1.0</outer_station_spanwise_position>
                <inner_chord_ratio>0.30</inner_chord_ratio>
				<outer_chord_ratio>0.30</outer_chord_ratio>
                <min_deflection unit="deg">-25.0</min_deflection>
				<max_deflection unit="deg">5.0</max_deflection>
            </symmetric_flap>
        </symmetric_flaps>
    </canard>
</jpad_config>