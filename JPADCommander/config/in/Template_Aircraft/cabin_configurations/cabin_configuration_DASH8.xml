<?xml version="1.0" encoding="utf-8"?>
<jpad_config>
    <configuration id="DASH-8 Cabin Configuration">
		<global_data>
			<actual_passengers_number>74</actual_passengers_number>
			<maximum_passengers_number>74</maximum_passengers_number>
			<flight_crew_number>2</flight_crew_number>
			<classes_number>1</classes_number>
			<classes_type>ECONOMY</classes_type>
			<aisles_number>1</aisles_number> <!-- numero di corridoi -->
			<x_coordinates_first_row unit="m">2.63</x_coordinates_first_row>
			<missing_seat_row>
				<!--
				The size of missing seat list has to be equal to the class size.
				If missing seat is empty, the seats number for that class is 
				given by rows*columns; otherwise it's given by 
				(rows*columns)-missing_seats_row.
				If the user wants to add other arrays it has to add another value tag.
				Each value tag contains an array of Integer values and must be assigned using [ , , ].
				-->
				<value>[0,2]</value>
			</missing_seat_row> 
		</global_data>
		<detailed_data>
			<number_of_breaks_economy_class>0</number_of_breaks_economy_class>
			<number_of_breaks_business_class>0</number_of_breaks_business_class>
			<number_of_breaks_first_class>0</number_of_breaks_first_class>
			<number_of_rows_economy_class>19</number_of_rows_economy_class>
			<number_of_rows_business_class>0</number_of_rows_business_class>
			<number_of_rows_first_class>0</number_of_rows_first_class>
			<number_of_columns_economy_class>2;2</number_of_columns_economy_class>
			<number_of_columns_business_class>0;0</number_of_columns_business_class>
			<number_of_columns_first_class>0;0</number_of_columns_first_class>
			<pitch_economy_class unit="m">0.80</pitch_economy_class>
			<pitch_business_class unit="m">0.0</pitch_business_class>
			<pitch_first_class unit="m">0.0</pitch_first_class>
			<width_economy_class unit="m">0.52</width_economy_class>
			<width_business_class unit="m">0.0</width_business_class>
			<width_first_class unit="m">0.0</width_first_class>
			<distance_from_wall_economy_class unit="m">0.1</distance_from_wall_economy_class>
			<distance_from_wall_business_class unit="m">0.0</distance_from_wall_business_class>
			<distance_from_wall_first_class unit="m">0.0</distance_from_wall_first_class>
		</detailed_data>
		<reference_masses>
			<mass_furnishings_and_equipment unit="kg">0.0</mass_furnishings_and_equipment>
		</reference_masses>
    </configuration>
</jpad_config>