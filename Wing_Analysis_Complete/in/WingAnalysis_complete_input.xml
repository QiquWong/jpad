<?xml version="1.0" encoding="utf-8"?>
<wing_executable>
        <operating_conditions>
                <altitude unit="m">6000</altitude>
                <mach_number>0.2</mach_number>
                <number_of_alpha>10</number_of_alpha>
                <alpha_initial unit="°">0.0</alpha_initial>
                <alpha_final unit="°">6.0</alpha_final>
        </operating_conditions>
       <wing>
            <global>
                <surface unit="m²">83.15</surface>
                <aspect_ratio>9.43</aspect_ratio>
                <number_of_point_semispan>100</number_of_point_semispan>
                <adimensional_kink_station>0.399</adimensional_kink_station>
				<momentum_pole>0.2</momentum_pole> <!-- with respect to MAC -->
             </global>
             <distribution>
                <number_of_given_sections>3</number_of_given_sections>
                <airfoil_family>NACA_65_SERIES</airfoil_family> <!-- NACA_4_DIGIT, NACA_5_DIGIT, NACA_63_SERIES, NACA_64_SERIES, NACA_65_SERIES, NACA_66_SERIES, BICONVEX, DOUBLE_WEDGE -->
				<airfoil_type>CONVENTIONAL</airfoil_type> <!-- CONVENTIONAL, PEAKY, SUPERCRITICAL, LAMINAR, MODERN_SUPERCRITICAL -->
				<max_thickness_mean_airfoil>0.15</max_thickness_mean_airfoil>
                   <geometry>
						<y_adimensional_stations>[0, 0.399, 1]</y_adimensional_stations>
						<chord_distribution unit="m">[6.39, 2.72, 1.05]</chord_distribution>
						<x_le_distribution unit="m">[0.0, 3.71, 8.31]</x_le_distribution>
						<twist_distribution unit="°">[0, -2, -6]</twist_distribution>
                    </geometry>
                    <aerodynamics>
						<alpha_zero_lift_distribution unit="°">[-1.98, -1.63, -3.18]</alpha_zero_lift_distribution>
						<alpha_star_distribution unit="°">[9.5, 8.5, 8.0]</alpha_star_distribution>
						<alpha_stall_distribution unit="°">[10, 10, 10]</alpha_stall_distribution>
						<cl_zero_distribution unit="°">[0.2, 0.2, 0.2]</cl_zero_distribution>
						<cl_alpha_distribution unit="1/°">[0.11, 0.11, 0.11]</cl_alpha_distribution> <!-- do not change unit -->
						<maximum_lift_coefficient_distribution>[2, 2, 1.8]</maximum_lift_coefficient_distribution>
						<cd_min_distribution>[0.006, 0.006, 0.006]</cd_min_distribution>
						<cl_ideal_distribution>[0.006, 0.006, 0.006]</cl_ideal_distribution>
						<k_distribution>[0.008, 0.008, 0.008]</k_distribution>
						<cm_c4_distribution>[-0.03, -0.03, -0.03]</cm_c4_distribution>
                    </aerodynamics>
             </distribution>
        </wing>
</wing_executable>