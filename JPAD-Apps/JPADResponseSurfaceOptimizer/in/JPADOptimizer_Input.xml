<?xml version="1.0" encoding="utf-8"?>
<JPAD_Optimizer>
	<number_of_variables>6</number_of_variables> 
	<number_of_objectives>5</number_of_objectives>
	<number_of_constraints>8</number_of_constraints>
	<max_number_of_evaluations>10000</max_number_of_evaluations> <!-- default value: 10000. May be left blank if the user wants to use the default value -->
	<population_size>1000</population_size> <!-- default value: 50. May be left blank if the user wants to use the default value -->
	<!--maximization_conditions: an array of booleans (TRUE=maximize, FALSE=minimize). The dimension must be equal to the number of objectives -->
	<maximization_conditions>FALSE;FALSE;FALSE;FALSE;FALSE</maximization_conditions> 
	<!--variable_lower_bounds: an array of double to define the lower bounds of each variable. The dimension must be equal to the number of variables -->
	<variable_lower_bounds>32.0;18.0;0.9;2.5;30.0;0.9</variable_lower_bounds>
	<!--variable_upper_bounds: an array of double to define the upper bounds of each variable. The dimension must be equal to the number of variables -->
	<variable_upper_bounds>35.5;22.0;1.1;10.0;32.0;1.1</variable_upper_bounds>
	<!--constraints: a set of constraints defined with multiple values. Each value must be decorated with a violating_condition chosen between the following:
		LESS_THAN, LESS_EQUAL_THAN, BIGGER_THAN, BIGGER_EQUAL_THAN, WITHIN_INTERVAL, OUTSIDE_INTERVAL
		The number of value tags must be equal to the number of constraints (if = 0, leave it blank)
		The code assigns a penalty to all solutions which satisfy the "violating_condition" attribute -->
	<constraints>
		<value violating_condition="OUTSIDE_INTERVAL">0.60;0.68</value>
		<value violating_condition="LESS_THAN">0.0</value>
		<value violating_condition="BIGGER_THAN">50.0</value>
		<value violating_condition="BIGGER_THAN">50.0</value>
		<value violating_condition="LESS_THAN">1.0</value>
		<value violating_condition="LESS_THAN">1.0</value>
		<value violating_condition="LESS_THAN">1.0</value>
		<value violating_condition="BIGGER_THAN">21.0</value>
	</constraints>
	<!--algorithms: an array of strings to assign all the wanted algorithms to be used. These will be used in sequence and compared -->
	<!-- possible values: see the MOEAFramework_Manual in the doc folder -->
	<algorithms>eNSGAII;OMOPSO</algorithms>
</JPAD_Optimizer>