<?xml version="1.0" encoding="utf-8"?>
<JPAD_Optimizer>
	<number_of_variables>4</number_of_variables> 
	<number_of_objectives>7</number_of_objectives>
	<number_of_constraints>3</number_of_constraints>
	<max_number_of_evaluations></max_number_of_evaluations> <!-- default value: 10000. May be left blank if the user wants to use the default value -->
	<population_size></population_size> <!-- default value: 50. May be left blank if the user wants to use the default value -->
	<!--maximization_conditions: an array of booleans (TRUE=maximize, FALSE=minimize). The dimension must be equal to the number of objectives -->
	<maximization_conditions>FALSE;FALSE;FALSE;FALSE;FALSE;FALSE;FALSE</maximization_conditions> 
	<!--variable_lower_bounds: an array of double to define the lower bounds of each variable. The dimension must be equal to the number of variables -->
	<variable_lower_bounds>15.0;9.5;10.2;25.0</variable_lower_bounds>
	<!--variable_upper_bounds: an array of double to define the upper bounds of each variable. The dimension must be equal to the number of variables -->
	<variable_upper_bounds>21.0;15.0;10.8;33.5</variable_upper_bounds>
	<!--constraints_values: an array of double to define the value of each constraints to be monitored. The dimension must be equal to the number of constraints (if = 0, leave it blank)-->
	<constraints_values>4.0;1169;1315</constraints_values>
	<!--constraints_violation_conditions: an array of strings to define the each constraints check condition. The dimension must be equal to the number of constraints -->
	<!-- possible values: LESS_THAN, LESS_EQUAL_THAN, BIGGER_THAN, BIGGER_EQUAL_THAN -->
	<constraints_violation_conditions>LESS_THAN;BIGGER_THAN;BIGGER_THAN</constraints_violation_conditions>
	<!--algorithms: an array of strings to assign all the wanted algorithms to be used. These will be used in sequence and compared -->
	<!-- possible values: see the MOEAFramework_Manual in the doc folder -->
	<algorithms>eNSGAII;OMOPSO</algorithms>
</JPAD_Optimizer>